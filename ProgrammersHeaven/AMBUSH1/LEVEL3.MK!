Gunswitch
T:
TOILET = INT(RND * 28) + 1
PIZZA = INT(RND * 28) + 1
IF TOILET = 10 OR TOILET = 20 OR TOILET = 30 THEN
IF TOILET = 40 OR TOILET = 50 THEN GOTO T
END IF
IF PIZZA = 10 OR PIZZA = 20 OR PIZZA = 30 THEN
IF PIZZA = 40 OR PIZZA = 50 THEN GOTO T
END IF
IF PIZZA = TOILET THEN GOTO T
LOCATE 1, 2: PRINT "Score :"; SCORE
LOCATE 1, 20: PRINT "Bombs :"; BOMB
