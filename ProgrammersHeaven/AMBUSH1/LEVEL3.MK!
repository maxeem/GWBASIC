IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF BAKEDBEANS = 500 THEN LOCATE 23, 2: PRINT "Beans :"; BEAN
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (X - 5, Y - 5), CIRC%, XOR
X1 = 150: X2 = 160: Y1 = 9: Y2 = 18
CC = 16
FOR I = 1 TO CHARGE
LINE (X1, Y1)-(X2, Y2), 30, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
X1 = X1 + 10: X2 = X2 + 10
CC = CC + 1.7
NEXT
X1 = 241: X2 = 249: Y1 = 9: Y2 = 18
FOR I = 1 TO 10 - CHARGE
LINE (X1, Y1)-(X2, Y2), 0, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), 0, BF
X1 = X1 - 10: X2 = X2 - 10
NEXT
IF LEVEL = 3 THEN PLAY "t255mbo3l4edcdeel2el4ddl2dl4egl2g"
INKY: Q$ = INKEY$
IF ADD2 > 0 THEN
PSET (SNOTTX, SNOTTY + ADD2), 120
PSET (SNOTTX + 1, SNOTTY + ADD1), 120
ADD1 = ADD1 + .05
ADD2 = ADD2 + .1
IF ADD2 >= 40 THEN ADD2 = 0: ADD1 = 0
END IF
IF TIME <> -1 AND LEVEL = 3 THEN GOSUB TIME
IF TIME <> -1 THEN TIME = TIME + 1
IF SONG = 0 AND TIME1 = 16 THEN
PLAY "t255mbo3l4edcdeeeddedl2c": TIME1 = 25
END IF
IF SONG = 0 AND TIME1 = 40 THEN TIME = -1
IF SONG = 1 AND TIME1 = 18 THEN
PLAY "T100MBL8O3L8GGFFEEL4DL8GGFFEEL4D": TIME1 = 19
END IF
IF SONG = 1 AND TIME1 = 35 THEN
PLAY "T100MBL8O3CCGGAAL4GL8FFEEDDL4C": TIME1 = 36
END IF
IF SONG = 1 AND TIME1 = 63 THEN TIME = -1
IF OUCHYOU >= 10 THEN Gameover: CHAIN "AMBUSH.bas"
IF GUN = 1 AND CHARGE < 10 AND Q$ = "c" THEN ChargeC: GOTO INKY
LOCATE 1, 2: PRINT "Score :"; SCORE
LOCATE 1, 20: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF BAKEDBEANS = 500 THEN LOCATE 23, 2: PRINT "Beans :"; BEAN
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
IF HITS = 90 AND LEVEL = 3 THEN
LINE (0, 22)-(319, 171), 0, BF
LOCATE 10, 13: PRINT "GET PSYCHED !!!"
CHAIN "BOSS3.mk!"
END IF
IF HITS = TOILET THEN GOTO ITEMS
IF HITS = PIZZA THEN GOTO ITEMS
IF AMMOA = 0 AND HITS = NEWAMMO THEN GOTO AMMO
IF Q$ = "" AND AMMOA > SPD THEN GOSUB AMMOERASE
IF Q$ = "" AND AMMOA > 0 THEN AMMOA = AMMOA + 1: ENEMY = -1: ITEM = -1
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 0 AND ENEMYTYPE = 3 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + 2
GOSUB GETHIT: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 0 AND ENEMYTYPE = 2 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + 1: GOSUB GETHIT
PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 0 AND ENEMYTYPE = 1 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + .5
GOSUB GETHIT: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 500 AND ENEMYTYPE = 3 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + 1.5
GOSUB GETHIT: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 500 AND ENEMYTYPE = 2 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + .75
GOSUB GETHIT: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 500 AND ENEMYTYPE = 1 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + .375
GOSUB GETHIT: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > 0 THEN ENEMY = ENEMY + 1: AMMOA = -1: ITEM = -1
IF Q$ = "" AND ITEM > SPD THEN GOSUB ITEMERASE
IF Q$ = "" AND ITEM > 0 THEN ITEM = ITEM + 1: ENEMY = -1: AMMOA = -1
IF Q$ = "" AND ENEMY = 0 THEN GOTO ENEMY

N1:
    Q$ = UCASE$(Q$)
    DEF SEG = 0
    POKE 1047, Numlock
    DEF SEG
IF LEN(Q$) = 2 THEN GOTO NXT
Q$ = LCASE$(Q$)
IF Q$ = CHR$(27) THEN
GET (0, 21)-(319, 172), BIG%
LOCATE 10, 14: COLOR 15: PRINT "Are you sure ?"
DO
END$ = INKEY$
LOOP UNTIL END$ <> ""
PUT (0, 21), BIG%, PSET
IF UCASE$(LEFT$(END$, 1)) = "Y" THEN CLS : END
COLOR 14
GOTO INKY
END IF
IF Q$ = " " THEN GOSUB FIRE
IF Q$ = "p" THEN GOSUB PAUSE
IF Q$ = "s" THEN Gunswitch
IF Q$ = "b" AND BOMB > 0 THEN GOSUB BOOM
IF Q$ = "1" OR Q$ = "2" OR Q$ = "3" OR Q$ = "4" OR Q$ = "5" OR Q$ = "6" THEN
IF Q$ = "1" THEN
GET (0, 21)-(319, 172), BIG%
COLOR 15
LOCATE 10, 11: PRINT "Stairway to Heaven"
