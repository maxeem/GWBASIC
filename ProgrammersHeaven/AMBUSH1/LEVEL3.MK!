BOOM:
GET (0, 21)-(319, 172), BIG%
FOR I = 1 TO 15
PLAY "MBT255L64O0Cp64O0Dp64O0Cp64"
NEXT
FOR NEWATTR = 0 TO 63
PALETTE 0, NEWATTR
FOR Q = 1 TO 25
NEXT
NEXT
FOR NEWATTR = 63 TO 0 STEP -1
PALETTE 0, NEWATTR
FOR Q = 1 TO 25
NEXT
NEXT
BOMBING = 1
BOMB = BOMB - 1
IF CHARACTER = 8 THEN BOMB = BOMB + 1
IF ENEMY > 1 THEN
HITS = HITS + 1
IF ENEMYTYPE = 1 THEN SCORE = SCORE + 10
IF ENEMYTYPE = 2 THEN SCORE = SCORE + 20
IF ENEMYTYPE = 3 THEN SCORE = SCORE + 30
PUT (0, 21), BIG%, PSET
GOSUB ENEMYERASE
PUT (X - 5, Y - 5), CIRC%, XOR
END IF
BOMBING = 0
RETURN

