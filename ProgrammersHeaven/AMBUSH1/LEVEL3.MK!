END IF
PUT (X - 5, Y - 5), CIRC%, XOR
PUT (EROW - 27, ECOL - 9), ENEMY%, PSET
IF ENEMY > SPD AND X = EROW AND Y = ECOL THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF ENEMY < SPD AND X = EROW AND Y = ECOL THEN
PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF ENEMY < SPD AND X = EROW AND Y = ECOL AND BOMBING = 1 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
END IF
EROW = 0
ECOL = 0
ENEMY = 0
AMMOA = 0
RETURN

