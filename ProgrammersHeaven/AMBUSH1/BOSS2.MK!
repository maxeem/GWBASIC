IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
GOSUB HITBOSS
PUT (X - 5, Y - 5), CIRC%, XOR
RETURN

HITBOSS:
IF BOSS = 150 THEN
X1 = 154: X2 = 169: Y1 = 26: Y2 = 44
FOR I = 1 TO OUCH
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), 12, BF
X1 = X1 + 15: X2 = X2 + 15
NEXT
END IF
IF BOSS = 100 THEN
X1 = 104: X2 = 119: Y1 = 26: Y2 = 44
FOR I = 1 TO OUCH
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), 12, BF
X1 = X1 + 15: X2 = X2 + 15
NEXT
END IF
RETURN

GUNSHOOT:
IF GUNKED <> 500 AND BOSS = 100 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
GET (0, 21)-(319, 172), BIG%
PLAY "t255l64o5ababababababababababababababababababab"
LINE (80, 45)-(199, 172), 0, BF
F = 150: G = 70
M = 10: Q = 15
FOR I = 1 TO 13
CIRCLE (F - 20 - M + 4, G - .5), M - 4, 0, , , -.4
CIRCLE (F + 20 + M - 4, G - .5), M - 4, 0, , , -.4
CIRCLE (F, G - .5), M - 8, 0
