DECLARE SUB Charge1 ()
DECLARE SUB Gameover ()
DECLARE SUB Center (ROW, TEXT$)
DECLARE SUB Gunswitch ()
COMMON NOGUNS, LEVEL, GUN, BOSS, X, Y, AMMO, SNOT, CHARGE, FLAME, BEANS
COMMON FLAMETHROWER, BAKEDBEANS, CHARACTER, CIRC%, BIG%, HIT%, EXTRA%
COMMON KEYB, FIRE, BEAN, HITS, MISSES, SCORE, BOMB, OUCHYOU, OUCH
COMMON INI, SPD, INI1$, INI2$, INI3$, TOILET, ENEMIES, ARMORA, BOW, ARROW
COMMON names, MISSLE, BAZOOKA, BZKA
RANDOMIZE TIMER
DIM ROW AS LONG
SCREEN 13
DIM CIRC%(1000)
DIM HIT%(1000)
DIM EXTRA%(1000)
DIM BIG%(25000)
BOSS = 100
CIRCLE (X, Y), 5, 15
GET (X - 5, Y - 5)-(X + 5, Y + 5), CIRC%
CIRCLE (X, Y), 5, 0
BOSS:
LOCATE 2, 1: PRINT STRING$(13, " ")
LOCATE 1, 1: PRINT STRING$(29, " ")
LINE (0, 21)-(319, 172), 0, BF
X1 = 104: X2 = 119: Y1 = 3: Y2 = 21
CC = 16
FOR I = 1 TO 10
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
X1 = X1 + 15: X2 = X2 + 15
CC = CC + 1.7
NEXT
LINE (0, 22)-(319, 172), 0, BF
GOSUB HEALTH
GOSUB TALKBOSS
CIRCLE (128, 70), 18, 4, , , -.6
CIRCLE (172, 70), 17, 4, , , -.6
CIRCLE (172, 70), 17, 4, , , -.6
PAINT (180, 70), 12, 4
PAINT (120, 70), 12, 4
CIRCLE (150, 60), 15, 4, , , 1
CIRCLE (150, 65), 10, 4, , , 1
LINE (141, 66)-(147, 68), 4
LINE (147, 68)-(153, 68), 4
LINE (153, 68)-(159, 66), 4
LINE (158, 71)-(152, 72), 4
LINE (152, 72)-(148, 72), 4
LINE (148, 72)-(142, 70), 4
LINE (118, 77)-(109, 91), 4
LINE (109, 91)-(111, 109), 4
LINE (111, 109)-(113, 112), 4
LINE (113, 112)-(118, 112), 4
LINE (118, 112)-(121, 108), 4
LINE (121, 108)-(123, 89), 4
LINE (123, 89)-(132, 91), 4
LINE (132, 91)-(142, 106), 4
LINE (142, 106)-(163, 106), 4
LINE (163, 106)-(171, 89), 4
LINE (171, 89)-(181, 93), 4
LINE (181, 93)-(184, 110), 4
LINE (184, 110)-(189, 113), 4
LINE (189, 113)-(193, 113), 4
LINE (193, 113)-(196, 110), 4
LINE (196, 110)-(192, 83), 4
LINE (192, 83)-(185, 75), 4
LINE (176, 77)-(168, 97), 4
LINE (168, 97)-(136, 97), 4
LINE (136, 97)-(124, 77), 4
LINE (142, 106)-(138, 134), 4
LINE (138, 134)-(140, 162), 4
LINE (139, 162)-(149, 168), 4, B
LINE (149, 162)-(153, 124), 4
LINE (153, 124)-(156, 140), 4
LINE (156, 140)-(158, 162), 4
LINE (157, 162)-(169, 167), 4, B
LINE (168, 162)-(167, 123), 4
LINE (167, 123)-(162, 106), 4
PAINT (151, 102), 65, 4
PAINT (151, 90), 12, 4
PAINT (116, 91), 65, 4
PAINT (186, 91), 65, 4
PAINT (149, 67), 65, 4
PAINT (149, 70), 65, 4
PAINT (149, 74), 65, 4
PAINT (149, 51), 15, 4
PAINT (149, 118), 12, 4
PAINT (144, 166), 14, 4
PAINT (160, 165), 14, 4
CIRCLE (147, 61), 2, 66
CIRCLE (153, 61), 2, 66
PAINT (147, 61), 15, 66
PAINT (153, 61), 15, 66
PSET (147, 61), 0
PSET (153, 61), 0
CIRCLE (150, 65), 1, 64
COLOR 14
LOCATE 11, 19: PRINT "27"
X1 = 104: X2 = 119: Y1 = 26: Y2 = 44
CC = 195
FOR I = 1 TO 10
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
X1 = X1 + 15: X2 = X2 + 15
CC = CC + 1.7
NEXT
PUT (X - 5, Y - 5), CIRC%, XOR:
PLAY "T155MBL8O1F#FF#FED#EFF#FF#FED#EF"
BOSSINKY: Q$ = INKEY$
IF GUN = 1 AND CHARGE < 10 AND Q$ = "c" THEN Charge1: GOTO BOSSINKY
IF OUCH >= 10 AND BOSS = 100 THEN GOTO KILLBOSS
IF OUCH >= 10 AND BOSS = 150 THEN GOTO KILLBOSS1
IF OUCH >= 1 THEN GOSUB HITBOSS
IF OUCHYOU >= 10 THEN Gameover
IF GUNSHOOT >= SPD + SPD / 6 THEN GOSUB GUNSHOOT
IF GUNSHOOT < SPD + SPD / 6 THEN GUNSHOOT = GUNSHOOT + 1
LOCATE 1, 2: PRINT "Score :"; SCORE
LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES

N2:
    Q$ = UCASE$(Q$)
    DEF SEG = 0
    POKE 1047, Numlock
    DEF SEG
IF LEN(Q$) = 2 THEN GOTO NXT1
Q$ = LCASE$(Q$)
IF Q$ = " " THEN GOSUB FIRE
IF Q$ = "p" THEN GOSUB PAUSE
IF Q$ = "s" THEN Gunswitch
IF Q$ = "b" AND BOMB > 0 THEN GOSUB BOOM
IF Q$ = CHR$(27) THEN
GET (0, 21)-(319, 172), BIG%
LOCATE 10, 14: COLOR 15: PRINT "Are you sure ?"
DO
END$ = INKEY$
LOOP UNTIL END$ <> ""
PUT (0, 21), BIG%, PSET
IF UCASE$(LEFT$(END$, 1)) = "Y" THEN CLS : END
COLOR 14
GOTO BOSSINKY
END IF
GOTO BOSSINKY
NXT1:
IF LEN(Q$) = 2 THEN FK% = ASC(RIGHT$(Q$, 1))
IF FK% <> 71 AND FK% <> 72 AND FK% <> 73 AND FK% <> 75 THEN
IF FK% <> 77 AND FK% <> 79 AND FK% <> 80 AND FK% <> 81 THEN GOTO BOSSINKY
END IF
IF FK% = 71 AND X = 30 OR FK% = 71 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 72 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 73 AND X = 280 OR FK% = 73 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 75 AND X = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR:  PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 77 AND X = 280 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 79 AND X = 30 OR FK% = 79 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 80 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 81 AND X = 280 OR FK% = 81 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 71 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: X = X - 10
Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 72 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 73 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 75 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: X = X - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 77 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: X = X + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 79 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X - 10: Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 80 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 81 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
GOTO BOSSINKY
PAUSE:
GET (0, 21)-(319, 172), BIG%
COLOR 15
LOCATE 10, 17: PRINT "PAUSE"
DO
AA$ = INKEY$
LOOP UNTIL UCASE$(AA$) = "P" OR UCASE$(AA$) = "S"
IF UCASE$(AA$) = "S" THEN PUT (0, 21), BIG%, PSET: Gunswitch: GOTO PAUSE
COLOR 14
PUT (0, 21), BIG%, PSET
RETURN

FIRE:
IF GUN = 1 AND AMMO = 0 THEN
PLAY "MBO6T255L64FG"
IF BOSS = 50 THEN CHARGE = -.2: Charge1
RETURN
END IF
IF GUN = 2 AND SNOT = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
IF FLAMETHROWER = 500 AND FIRE = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
FOR I = 1 TO CHARGE
PLAY "MBT255L64O0Cp64O0Dp64O0Cp64"
NEXT
IF CHARGE >= 1 THEN
GET (X - 20, Y - 20)-(X + 20, Y + 20), EXTRA%
HGLK = CHARGE
FOR I = 1 TO CHARGE + 10
CIRCLE (X, Y), HGLK + 10, HGLK + 21
HGLK = HGLK - 1
NEXT
PUT (X - 20, Y - 20), EXTRA%, PSET
END IF
GET (X - 5, Y - 5)-(X + 5, Y + 5), HIT%
CIRCLE (X, Y), 3, 18
CIRCLE (X, Y), 2, 18
CIRCLE (X, Y), 1, 18
CIRCLE (X, Y), 3, 19
CIRCLE (X, Y), 2, 30
CIRCLE (X, Y), 1, 30
CIRCLE (X, Y), 3, 20
CIRCLE (X, Y), 2, 29
CIRCLE (X, Y), 1, 29
CIRCLE (X, Y), 3, 28
IF CHARACTER = 5 AND GUN = 1 THEN
PLAY "MBT255L32O5CEDFEGFAGBA>C<B>DCED": GGGGG = 2
END IF
IF BEANS = 0 THEN GGGGG = 2
IF GGGGG = 2 THEN GOTO W
IF GUN = 1 THEN PLAY "MBO0T255L64CDCDCDCDCDCD"
IF GUN = 2 THEN PLAY "T255O1L64CDEFG"
IF FLAMETHROWER = 500 THEN PLAY "T255MBL64O1CO0AO0CO0AO1AO0C"
W:
GGGGG = 0
PUT (X - 5, Y - 5), HIT%, PSET
IF GUN = 2 THEN
CIRCLE (X, Y), 2, 120
PAINT (X, Y), 120
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 120
IF A = 2 THEN PSET (X - RX, Y + RY), 120
NEXT
END IF
IF GUN = 1 AND CHARACTER = 5 THEN
GET (X - 20, Y - 10)-(X + 20, Y + 10), EXTRA%
Q = 4
M = 27
FOR I = 1 TO 5
CIRCLE (X - 4, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X - 4, Y + 4), Q, M, 1.570796, 6.283184, 1
CIRCLE (X + 4, Y + 4), Q, M, 3.141592, 1.570796, 1
CIRCLE (X + 4, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X - 14, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X - 14, Y + 4), Q, M, 1.570796, 6.283184, 1
CIRCLE (X + 14, Y + 4), Q, M, 3.141592, 1.570796, 1
CIRCLE (X + 14, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X + 14, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X + 14, Y + 4), Q, M, 1.570796, 6.283184, 1
CIRCLE (X - 14, Y + 4), Q, M, 3.141592, 1.570796, 1
CIRCLE (X - 14, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X, Y), Q, M
CIRCLE (X - 9, Y), Q, M
CIRCLE (X + 9, Y), Q, M
Q = Q - 1
M = M + 1
NEXT
PUT (X - 20, Y - 10), EXTRA%, PSET
END IF
IF FLAMETHROWER = 500 THEN
PUT (X - 5, Y - 5), CIRC%
Q = 2.2: QQQ = .3: QQQQ = 33
FOR I = 1 TO 22
CIRCLE (X, Y + 1), QQQ, QQQQ, , , Q
QQQ = QQQ + .3: QQQQ = QQQQ + .5: Q = Q - .05
NEXT I
Q = 0: QQQ = .1: QQQQ = 36
FOR I = 1 TO 22
CIRCLE (X, Y), QQQ, QQQQ, , , Q
QQQ = QQQ + .39: QQQQ = QQQQ + .4: Q = Q + .1
NEXT I
PUT (X - 5, Y - 5), CIRC%
END IF
IF X = 150 AND Y = 60 AND BOSS = 100 AND GUN = 1 THEN
IF HIT >= 4.2 THEN
HIT = HIT - .2 - CHARGE / 12
MISSES = MISSES + 1
OUCH = OUCH - .15 - CHARGE / 12
END IF
OUCH = OUCH + .15 + CHARGE / 12
HIT = HIT + .2 + CHARGE / 12
CIRCLE (X, Y), HIT, 4
PAINT (X, Y), 4
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 4
IF A = 2 THEN PSET (X - RX, Y + RY), 4
NEXT
END IF
IF X = 150 AND Y = 80 AND FLAMETHROWER = 500 AND QQQ1 <= 2 THEN
IF BOSS = 100 OR X = 150 AND Y = 90 AND FLAMETHROWER = 500 THEN
IF QQQ1 <= 2 AND BOSS = 100 THEN
IF QQQ1 >= 2 THEN
QQQ1 = QQQ1 - .5
MISSES = MISSES + 1
OUCH = OUCH - 1
END IF
END IF
END IF
'END IF
OUCH = OUCH + 1
PUT (X - 5, Y - 5), CIRC%
XX = X: YY = Y
FOR h = 1 TO 10
IF h = 1 THEN YY = 90: XX = 140
IF h = 2 THEN XX = 150
IF h = 3 THEN XX = 160
IF h = 4 THEN YY = 80: XX = 140
IF h = 5 THEN XX = 150
IF h = 6 THEN XX = 160
IF h = 7 THEN YY = 70: XX = 130
IF h = 8 THEN XX = 170
IF h = 9 THEN XX = 120
IF h = 10 THEN XX = 180
Q = 2.2: QQQQ = 33
FOR I = 1 TO 22
CIRCLE (XX, YY + 1), QQQ1, QQQQ, , , Q
QQQ1 = QQQ1 + .3: QQQQ = QQQQ + .5: Q = Q - .05
NEXT I
QQQ1 = QQQ1 - 6.6
Q = 0: QQQQ = 36
FOR I = 1 TO 22
CIRCLE (XX, YY), QQQ1, QQQQ, , , Q
QQQ1 = QQQ1 + .39: QQQQ = QQQQ + .4: Q = Q + .1
NEXT I
QQQ1 = QQQ1 - 8.58
NEXT h
PUT (X - 5, Y - 5), CIRC%
QQQ1 = QQQ1 + .5
END IF
IF GUN = 2 AND X >= 140 AND X <= 170 AND Y = 160 AND BOSS = 100 THEN
IF SNT <= 11 THEN
XX = 140: YY = 160
OUCH = OUCH + .25
FOR D = 1 TO 4
CIRCLE (XX, YY), SNT, 120
PAINT (XX, YY), 120
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (XX + RX, YY - RY), 120
IF A = 2 THEN PSET (XX - RX, YY + RY), 120
NEXT I
XX = XX + 10
NEXT D
SNT = SNT + 1
IF SNT > 11 THEN GUNKED = 500
END IF
END IF
IF X = 130 AND Y = 80 AND BOSS = 150 AND GUN = 1 AND CHARGE >= 10 THEN
IF HIT >= 4 THEN
HIT = HIT - .2
MISSES = MISSES + 1
OUCH = OUCH - .15 - CHARGE / 12
END IF
OUCH = OUCH + .15 + CHARGE / 12
HIT = HIT + .2
CIRCLE (X, Y), HIT, 4
PAINT (X, Y), 4
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 4
IF A = 2 THEN PSET (X - RX, Y + RY), 4
NEXT
END IF
IF GUN = 1 THEN AMMO = AMMO - 1
IF GUN = 2 THEN SNOT = SNOT - 1
IF FLAMETHROWER = 500 THEN FIRE = FIRE - 1
IF GUN = 1 AND CHARACTER = 8 THEN AMMO = AMMO + 1
IF GUN = 2 AND CHARACTER = 8 THEN SNOT = SNOT + 1
IF FLAMETHROWER = 500 AND CHARACTER = 8 THEN FIRE = FIRE + 1
IF BOSS = 100 THEN CHARGE = -.2: Charge1
IF BOSS = 150 THEN CHARGE = -.2: Charge1
RETURN

BOOM:
GET (0, 21)-(319, 172), BIG%
FOR I = 1 TO 15
PLAY "MBT255L64O0Cp64O0Dp64O0Cp64"
NEXT
FOR NEWATTR = 0 TO 63
PALETTE 0, NEWATTR
FOR Q = 1 TO 25
NEXT
NEXT
FOR NEWATTR = 63 TO 0 STEP -1
PALETTE 0, NEWATTR
FOR Q = 1 TO 25
NEXT
NEXT
BOMBING = 1
BOMB = BOMB - 1
IF CHARACTER = 8 THEN BOMB = BOMB + 1
BOMBING = 0
RETURN

GETHIT:
GET (0, 21)-(319, 172), BIG%
PLAY "T255MBL64O0CDCDCDCDCDCDCDCDCDCDCDC"
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
