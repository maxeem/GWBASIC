DECLARE SUB Gameover ()
DECLARE SUB Charge1 ()
DECLARE SUB Gunswitch ()
DECLARE SUB Center (ROW, TEXT$)
COMMON NOGUNS, LEVEL, GUN, BOSS, X, Y, AMMO, SNOT, CHARGE, FLAME, BEANS
COMMON FLAMETHROWER, BAKEDBEANS, CHARACTER, CIRC%, BIG%, HIT%, EXTRA%
COMMON KEYB, FIRE, BEAN, HITS, MISSES, SCORE, BOMB, OUCHYOU, OUCH
COMMON INI, SPD, INI1$, INI2$, INI3$, TOILET, ENEMIES, ARMORA, BOW, ARROW
COMMON NAMES, MISSLE, BAZOOKA, BZKA
DIM CIRC%(1000)
DIM HIT%(1000)
DIM EXTRA%(1000)
DIM BIG%(24500)
SCREEN 13
CIRCLE (X, Y), 5, 15
GET (X - 5, Y - 5)-(X + 5, Y + 5), CIRC%
CIRCLE (X, Y), 5, 0
BOSS:
LOCATE 2, 1: PRINT STRING$(13, " ")
LOCATE 1, 1: PRINT STRING$(29, " ")
LINE (0, 21)-(319, 172), 0, BF
X1 = 104: X2 = 119: Y1 = 3: Y2 = 21
CC = 16
FOR I = 1 TO 10
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
X1 = X1 + 15: X2 = X2 + 15
CC = CC + 1.7
NEXT
GOSUB HEALTH
BOSS = 50
BOSSA = 150: BOSSB = 70
N:
LINE (0, 22)-(319, 172), 0, BF
CIRCLE (BOSSA + 5, BOSSB + 5), 25, 15, , , 1
PAINT (BOSSA + 5, BOSSB + 5), 66, 15
CIRCLE (BOSSA + 5, BOSSB + 10), 3, 64, , , 1
PAINT (BOSSA + 5, BOSSB + 10), 65, 64
CIRCLE (BOSSA + 5, BOSSB + 22), 8, 0, , , -.3
PAINT (BOSSA + 5, BOSSB + 22), 18, 0
CIRCLE (BOSSA, BOSSB), 4, 0, , , 1.2
CIRCLE (BOSSA + 10, BOSSB), 4, 0, , , 1.2
PAINT (BOSSA, BOSSB), 28, 0
PAINT (BOSSA + 10, BOSSB), 28, 0
PSET (BOSSA, BOSSB), 1
PSET (BOSSA + 10, BOSSB), 1
LINE (BOSSA + 17, BOSSB - 8)-(BOSSA + 7, BOSSB - 5), 0
LINE (BOSSA + 4, BOSSB - 5)-(BOSSA - 6, BOSSB - 8), 0
LINE (BOSSA - 3, BOSSB - 14)-(BOSSA + 2, BOSSB - 17), 0
LINE (BOSSA + 2, BOSSB - 14)-(BOSSA + 8, BOSSB - 17), 0
LINE (BOSSA + 25, BOSSB - 6)-(BOSSA + 23, BOSSB - 3), 0
LINE (BOSSA - 12, BOSSB - 3)-(BOSSA - 15, BOSSB - 6), 0
LINE (BOSSA - 16, BOSSB)-(BOSSA - 20, BOSSB - 1), 0
LINE (BOSSA + 26, BOSSB)-(BOSSA + 30, BOSSB - 1), 0

LINE (BOSSA - 28, BOSSB + 61)-(BOSSA - 42, BOSSB + 34), 15
LINE (BOSSA + 50, BOSSB + 34)-(BOSSA + 37, BOSSB + 61), 15
CIRCLE (BOSSA + 48, BOSSB + 23), 23, 15, , , -.4
PAINT (BOSSA + 48, BOSSB + 23), 15
CIRCLE (BOSSA + 68, BOSSB + 37), 19, 15, , , 1.6
PAINT (BOSSA + 68, BOSSB + 37), 65, 15
CIRCLE (BOSSA - 38, BOSSB + 23), 23, 15, , , -.4
PAINT (BOSSA - 38, BOSSB + 23), 15
CIRCLE (BOSSA - 57, BOSSB + 37), 19, 15, , , 1.6
PAINT (BOSSA - 57, BOSSB + 37), 65, 15
CIRCLE (BOSSA + 5, BOSSB + 69), 51, 15, , , -.4
LINE (BOSSA + 49, BOSSB + 9)-(BOSSA + 29, BOSSB + 11), 15
LINE (BOSSA - 19, BOSSB + 11)-(BOSSA - 43, BOSSB + 9), 15
LINE (BOSSA - 49, BOSSB + 63)-(BOSSA - 68, BOSSB + 49), 15
LINE (BOSSA + 55, BOSSB + 63)-(BOSSA + 78, BOSSB + 52), 15
PAINT (BOSSA + 5, BOSSB + 31), 15
PAINT (BOSSA + 5, BOSSB + 69), 15
CIRCLE (BOSSA + 52, BOSSB + 48), 15, 19, , , 1
CIRCLE (BOSSA - 44, BOSSB + 48), 15, 19, , , 1
PAINT (BOSSA + 52, BOSSB + 48), 18, 19
PAINT (BOSSA - 44, BOSSB + 48), 18, 19
PAINT (BOSSA + 30, BOSSB + 13), 15
PAINT (BOSSA - 20, BOSSB + 13), 15
GOSUB TALKBOSS
X1 = 104: X2 = 119: Y1 = 26: Y2 = 44
CC = 195
FOR I = 1 TO 10
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
X1 = X1 + 15: X2 = X2 + 15
CC = CC + 1.7
NEXT
PUT (X - 5, Y - 5), CIRC%, XOR:
PLAY "T155MBL8O1F#FF#FED#EFF#FF#FED#EF"
BOSSINKY: Q$ = INKEY$
IF GUN = 1 AND CHARGE < 10 AND Q$ = "c" THEN Charge1: GOTO BOSSINKY
IF OUCH >= 10 THEN GOTO KILLBOSS
IF OUCH >= 1 THEN GOSUB HITBOSS
IF OUCHYOU >= 10 THEN Gameover
IF GUNSHOOT >= SPD THEN GOSUB GUNSHOOT
IF GUNSHOOT < SPD THEN GUNSHOOT = GUNSHOOT + 1
LOCATE 1, 2: PRINT "Score :"; SCORE
LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES

N2:
    Q$ = UCASE$(Q$)
    DEF SEG = 0
    POKE 1047, Numlock
    DEF SEG
IF LEN(Q$) = 2 THEN GOTO NXT1
Q$ = LCASE$(Q$)
IF Q$ = " " THEN GOSUB FIRE
IF Q$ = "p" THEN GOSUB PAUSE
IF Q$ = "s" THEN Gunswitch
IF Q$ = "b" AND BOMB > 0 THEN GOSUB BOOM
IF Q$ = CHR$(27) THEN
GET (0, 21)-(319, 172), BIG%
LOCATE 10, 14: COLOR 15: PRINT "Are you sure ?"
DO
END$ = INKEY$
LOOP UNTIL END$ <> ""
PUT (0, 21), BIG%, PSET
IF UCASE$(LEFT$(END$, 1)) = "Y" THEN CLS : END
COLOR 14
GOTO BOSSINKY
END IF
GOTO BOSSINKY
NXT1:
IF LEN(Q$) = 2 THEN FK% = ASC(RIGHT$(Q$, 1))
IF FK% <> 71 AND FK% <> 72 AND FK% <> 73 AND FK% <> 75 THEN
IF FK% <> 77 AND FK% <> 79 AND FK% <> 80 AND FK% <> 81 THEN GOTO BOSSINKY
END IF
IF FK% = 71 AND X = 30 OR FK% = 71 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 72 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 73 AND X = 280 OR FK% = 73 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 75 AND X = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR:  PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 77 AND X = 280 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 79 AND X = 30 OR FK% = 79 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 80 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 81 AND X = 280 OR FK% = 81 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 71 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X - 10: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 72 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 73 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 75 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: X = X - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 77 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 79 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X - 10: Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 80 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 81 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
GOTO BOSSINKY
PAUSE:
GET (0, 21)-(319, 172), BIG%
COLOR 15
LOCATE 10, 17: PRINT "PAUSE"
DO
AA$ = INKEY$
LOOP UNTIL UCASE$(AA$) = "P" OR UCASE$(AA$) = "S"
IF UCASE$(AA$) = "S" THEN PUT (0, 21), BIG%, PSET: Gunswitch: GOTO PAUSE
COLOR 14
PUT (0, 21), BIG%, PSET
RETURN

FIRE:
IF GUN = 1 AND AMMO = 0 THEN
PLAY "MBO6T255L64FG"
IF BOSS = 50 THEN CHARGE = -.2: Charge1
RETURN
END IF
IF GUN = 2 AND SNOT = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
FOR I = 1 TO CHARGE
PLAY "MBT255L64O0Cp64O0Dp64O0Cp64"
NEXT
IF CHARGE >= 1 THEN
GET (X - 20, Y - 20)-(X + 20, Y + 20), EXTRA%
HGLK = CHARGE
FOR I = 1 TO CHARGE + 10
CIRCLE (X, Y), HGLK + 10, HGLK + 21
HGLK = HGLK - 1
NEXT
PUT (X - 20, Y - 20), EXTRA%, PSET
END IF
GET (X - 5, Y - 5)-(X + 5, Y + 5), HIT%
CIRCLE (X, Y), 3, 18
CIRCLE (X, Y), 2, 18
CIRCLE (X, Y), 1, 18
CIRCLE (X, Y), 3, 19
CIRCLE (X, Y), 2, 30
CIRCLE (X, Y), 1, 30
CIRCLE (X, Y), 3, 20
CIRCLE (X, Y), 2, 29
CIRCLE (X, Y), 1, 29
CIRCLE (X, Y), 3, 28
IF CHARACTER = 5 AND GUN = 1 THEN
PLAY "MBT255L32O5CEDFEGFAGBA>C<B>DCED": GGGGG = 2
END IF
IF BEANS = 0 THEN GGGGG = 2
IF GGGGG = 2 THEN GOTO W
IF GUN = 1 THEN PLAY "MBO0T255L64CDCDCDCDCDCD"
IF GUN = 2 THEN PLAY "T255O1L64CDEFG"
W:
GGGGG = 0
PUT (X - 5, Y - 5), HIT%, PSET
IF X <> EROW AND Y < 280 AND Y > 20 THEN
IF GUN = 2 OR Y <> ECOL AND Y < 280 AND Y > 20 AND GUN = 2 THEN
CIRCLE (X, Y), 2, 120
PAINT (X, Y), 120
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 120
IF A = 2 THEN PSET (X - RX, Y + RY), 120
NEXT
END IF
END IF
IF X <> EROW AND Y < 280 AND Y > 20 AND CHARACTER = 5 THEN
IF GUN = 1 OR Y <> ECOL AND Y < 280 AND Y > 20 AND CHARACTER = 5 THEN
IF GUN = 1 THEN
GET (X - 20, Y - 10)-(X + 20, Y + 10), EXTRA%
Q = 4
M = 27
FOR I = 1 TO 5
CIRCLE (X - 4, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X - 4, Y + 4), Q, M, 1.570796, 6.283184, 1
CIRCLE (X + 4, Y + 4), Q, M, 3.141592, 1.570796, 1
CIRCLE (X + 4, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X - 14, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X - 14, Y + 4), Q, M, 1.570796, 6.283184, 1
CIRCLE (X + 14, Y + 4), Q, M, 3.141592, 1.570796, 1
CIRCLE (X + 14, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X + 14, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X + 14, Y + 4), Q, M, 1.570796, 6.283184, 1
CIRCLE (X - 14, Y + 4), Q, M, 3.141592, 1.570796, 1
CIRCLE (X - 14, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X, Y), Q, M
CIRCLE (X - 9, Y), Q, M
CIRCLE (X + 9, Y), Q, M
Q = Q - 1
M = M + 1
NEXT
PUT (X - 20, Y - 10), EXTRA%, PSET
END IF
END IF
END IF
IF LEVEL = 1 AND GUN = 2 AND BOSS = 50 AND X = 200 AND Y = 120 THEN
IF SNT >= 20 THEN
GUNKED = 50
SNT = SNT - 2
MISSES = MISSES + 1
OUCH = OUCH - .3
END IF
OUCH = OUCH + .3
SNT = SNT + 2
CIRCLE (X, Y), SNT - 1, 120
CIRCLE (X, Y), SNT, 120
PAINT (X, Y), 120
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 120
IF A = 2 THEN PSET (X - RX, Y + RY), 120
NEXT
END IF
IF LEVEL = 1 AND GUN = 2 AND BOSS = 50 AND X = 110 AND Y = 120 THEN
IF SNT1 >= 20 THEN
GUNKED1 = 50
SNT1 = SNT1 - 2
MISSES = MISSES + 1
OUCH = OUCH - .3
END IF
OUCH = OUCH + .3
SNT1 = SNT1 + 2
CIRCLE (X, Y), SNT1 - 1, 120
CIRCLE (X, Y), SNT1, 120
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 120
IF A = 2 THEN PSET (X - RX, Y + RY), 120
NEXT
END IF
IF LEVEL = 1 AND GUN = 1 AND BOSS = 50 AND X = 150 AND Y = 70 THEN
IF HIT >= 3 THEN
HIT = HIT - .2 - CHARGE / 12
MISSES = MISSES + 1
OUCH = OUCH - .15 - CHARGE / 12
END IF
OUCH = OUCH + .15 + CHARGE / 12
HIT = HIT + .2 + CHARGE / 12
CIRCLE (X, Y), HIT, 4
PAINT (X, Y), 4
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 4
IF A = 2 THEN PSET (X - RX, Y + RY), 4
NEXT
END IF
IF LEVEL = 1 AND GUN = 1 AND BOSS = 50 AND X = 160 AND Y = 70 THEN
IF HIT1 >= 3 THEN
HIT1 = HIT1 - .2 - CHARGE / 12
MISSES = MISSES + 1
OUCH = OUCH - .15 - CHARGE / 12
END IF
OUCH = OUCH + .15 + CHARGE / 12
HIT1 = HIT1 + .2 + CHARGE / 12
CIRCLE (X, Y), HIT1, 4
PAINT (X, Y), 4
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 4
IF A = 2 THEN PSET (X - RX, Y + RY), 4
NEXT
END IF
IF GUN = 1 THEN AMMO = AMMO - 1
IF GUN = 2 THEN SNOT = SNOT - 1
IF GUN = 1 AND CHARACTER = 8 THEN AMMO = AMMO + 1
IF GUN = 2 AND CHARACTER = 8 THEN SNOT = SNOT + 1
IF BOSS = 50 THEN CHARGE = -.2: Charge1
RETURN

BOOM:
GET (0, 21)-(319, 172), BIG%
FOR I = 1 TO 15
PLAY "MBT255L64O0Cp64O0Dp64O0Cp64"
NEXT
FOR NEWATTR = 0 TO 63
PALETTE 0, NEWATTR
FOR Q = 1 TO 25
NEXT
NEXT
FOR NEWATTR = 63 TO 0 STEP -1
PALETTE 0, NEWATTR
FOR Q = 1 TO 25
NEXT
NEXT
BOMBING = 1
BOMB = BOMB - 1
IF CHARACTER = 8 THEN BOMB = BOMB + 1
BOMBING = 0
RETURN

GETHIT:
GET (0, 21)-(319, 172), BIG%
PLAY "T255MBL64O0CDCDCDCDCDCDCDCDCDCDCDC"
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 173)-(319, 200), 0, BF
IF BOSS = 50 THEN LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
RETURN

HITBOSS:
X1 = 104: X2 = 119: Y1 = 26: Y2 = 44
FOR I = 1 TO OUCH
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), 12, BF
X1 = X1 + 15: X2 = X2 + 15
NEXT
RETURN

GUNSHOOT:
IF GUNKED1 <> 50 THEN
GET (BOSSA - 23, BOSSB + 27)-(BOSSA - 65, BOSSB + 68), EXTRA%
CIRCLE (BOSSA - 44, BOSSB + 48), 20, 22, , , 1
PAINT (BOSSA - 44, BOSSB + 48), 14, 22
FOR I = 1 TO SPD
NEXT
PUT (BOSSA - 65, BOSSB + 27), EXTRA%, PSET
OUCHYOU = OUCHYOU + 1
GOSUB GETHIT
END IF

IF GUNKED <> 50 THEN
GET (BOSSA + 31, BOSSB + 27)-(BOSSA + 73, BOSSB + 68), EXTRA%
CIRCLE (BOSSA + 52, BOSSB + 48), 20, 22, , , 1
PAINT (BOSSA + 52, BOSSB + 48), 14, 22
FOR I = 1 TO SPD
NEXT
PUT (BOSSA + 31, BOSSB + 27), EXTRA%, PSET
OUCHYOU = OUCHYOU + 1
GOSUB GETHIT
END IF

X1 = 104: X2 = 119: Y1 = 3: Y2 = 21
FOR I = 1 TO OUCHYOU
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), 12, BF
X1 = X1 + 15: X2 = X2 + 15
NEXT

GUNSHOOT = 0
RETURN
TALKBOSS:
COLOR 14
LOCATE 5, 15: PRINT "Come Here! "
PLAY "T155MFL8O3F#FF#FED#EFF#FF#FED#EF"
LOCATE 5, 1: PRINT STRING$(39, " ")
RETURN

KILLBOSS:
GOSUB HITBOSS
BOSS = 0
GUNKED = 0
GUNKED1 = 0
SNT = 0
SNT1 = 0
HIT = 0
HIT1 = 0
OUCHYOU = 0
OUCH = 0
GUNSHOOT = 0
Q = 0
BOSSA1 = BOSSA + 10
FOR I = 1 TO 100
C = INT(RND * 3)
C1 = INT(RND * 3)
IF C = 1 THEN C = -1
IF C = 2 THEN C = 1
IF C1 = 1 THEN C1 = -1
IF C1 = 2 THEN C1 = 1
IF CC = 2 THEN CC = -1
CC = INT(RND * 3) + 1
IF CC = 1 THEN CC = 39
IF CC = 2 THEN CC = 40
IF CC = 3 THEN CC = 41
IF CC = 4 THEN CC = 42
PSET (BOSSA + C, BOSSB + I), CC
PSET (BOSSA1 + C1, BOSSB + I), CC
PSET (BOSSA + C - 1, BOSSB + I), CC - 1
PSET (BOSSA1 + C1 + 1, BOSSB + I), CC - 1
BOSSA = BOSSA + C
BOSSA1 = BOSSA1 + C1
PLAY "T55MFP32"
NEXT
FOR I = 1 TO 350
W = INT(RND * 80) + 1
WA = INT(RND * 80) + 1
CIRCLE (BOSSA + W, BOSSB + WA), 15, 0, , , 1
CIRCLE (BOSSA - WA, BOSSB + W), 15, 0, , , 1
PLAY "T255MBL64O0CD"
NEXT
LINE (0, 22)-(319, 171), 0, BF
PLAY "T255P1"
CHAINPLACE = 1
GOSUB nextlevel
CHAIN "level2.mk!"

HEALTH:
X1 = 104: X2 = 119: Y1 = 3: Y2 = 21
CC = 16
FOR I = 1 TO 10
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
X1 = X1 + 15: X2 = X2 + 15
CC = CC + 1.7
NEXT
X1 = 104: X2 = 119: Y1 = 3: Y2 = 21
FOR I = 1 TO OUCHYOU
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), 12, BF
X1 = X1 + 15: X2 = X2 + 15
NEXT
RETURN

nextlevel:
SCREEN 0
WIDTH 80
100 CLS
COLOR 14
Center 2, "Good Job !!!"
COLOR 15
Center 6, "You have successfully completed your mission in the rampage at Sou"
LOCATE 6, 74: PRINT "th "; ""
Center 8, "Range. You have killed 30 of those annoying little brats along wit"
LOCATE 8, 74: PRINT "h"
IF NAMES = 0 THEN
Center 10, "Mr. Baldwin! You have gained the flame thrower. [ENTER]"
END IF
IF NAMES = 500 THEN
Center 10, "Mr. Krollop! You have gained the flame thrower. [ENTER]"
END IF
HITS = HITS + 1
FLAME = 500: NOGUNS = 3: FIRE = 20
DO
AA$ = INKEY$
LOOP UNTIL AA$ = CHR$(13)
CLS
COLOR 14
Center 2, "Level 1 Stats"
COLOR 15
LOCATE 5, 9: PRINT "Kills: "; HITS
LOCATE 7, 9: PRINT "Misses: "; MISSES
LOCATE 9, 9: PRINT "Kill%: "; : PRINT USING "#.###"; HITS / (MISSES + HITS)
IF MISSES = 0 THEN LOCATE 9, 9: PRINT "Kill%:  1.000"
LOCATE 11, 9: PRINT "Score: "; SCORE
LOCATE 13, 9: PRINT "Weapons and items captured:"
LOCATE 15, 9: PRINT "Uzi   SnotRocket   ";
IF TOILET = 500 THEN PRINT "Toilet   ";
IF FLAME = 500 THEN PRINT "Flame   ";
IF BEANS = 500 THEN PRINT "Beans   ";
IF BOW = 500 THEN PRINT "Crossbow   ";
Center 21, "On to the next level!!"
DO
AA$ = INKEY$
LOOP UNTIL AA$ <> ""
LEVEL = LEVEL + 1
ENEMIES = LEVEL
CLS
Center 12, "Please wait"
RETURN

SUB Center (ROW, TEXT$)
    SHARED KEYB
    IF KEYB <> 3 THEN
    LOCATE ROW, 41 - LEN(TEXT$) / 2
    PRINT TEXT$;
    END IF
    IF KEYB = 3 THEN
    LOCATE ROW, 18 - LEN(TEXT$) / 2
    PRINT TEXT$;
    END IF
END SUB

SUB Charge1
SHARED CHARGE
CHARGE = CHARGE + .2
X1 = 10: X2 = 20: Y1 = 139: Y2 = 148
FOR I = 1 TO 10 - CHARGE
LINE (X1, Y1)-(X2, Y2), 0, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), 0, BF
Y1 = Y1 - 10: Y2 = Y2 - 10
NEXT
X1 = 10: X2 = 20: Y1 = 49: Y2 = 58
CC = 16
FOR I = 1 TO CHARGE
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
Y1 = Y1 + 10: Y2 = Y2 + 10
CC = CC + 1.7
NEXT
END SUB

SUB Gameover
SHARED SPD, SCORE, INI1$, INI2$, INI3$, HITS
COLOR 15
FOR I = 1 TO SPD * 4: NEXT: CLS : SCREEN 13
FOR I = 2 TO 13:  LOCATE I, 16: PRINT "G": LOCATE I - 1, 16: PRINT " "
FOR A = 1 TO SPD: NEXT: NEXT
PLAY "t255mbl64o0cdcdcdcdcdcdcdcdp16cp64ddcp32d"
FOR I = 2 TO 13:  LOCATE I, 18: PRINT "A": LOCATE I - 1, 18: PRINT " "
FOR A = 1 TO SPD: NEXT: NEXT
PLAY "t255mbl64o0cdcdcdcdcdcdcdcdp16cp32ddcp64d"
FOR I = 2 TO 13:  LOCATE I, 20: PRINT "M": LOCATE I - 1, 20: PRINT " "
FOR A = 1 TO SPD: NEXT: NEXT
PLAY "t255mbl64o0cdcdcdcdcdcdcdcdp16cp32ddcp64d"
FOR I = 2 TO 13:  LOCATE I, 22: PRINT "E": LOCATE I - 1, 22: PRINT " "
FOR A = 1 TO SPD: NEXT: NEXT
PLAY "t255mbl64o0cdcdcdcdcdcdcdcdp16cp32ddcp64d"
FOR I = 33 TO 23 STEP -1: LOCATE 13, I
PRINT "O V E R": LOCATE 13, I + 7: PRINT " "
FOR A = 1 TO SPD: NEXT: NEXT
PLAY "t255mbl64o0cdcdcdcdcdcdcdcdp16cp32ddcp64d"
FOR I = 15 TO 12 STEP -1: LOCATE 13, I
PRINT "G A M E": LOCATE 13, I + 7: PRINT " "
FOR A = 1 TO SPD * 4: NEXT: NEXT
DO
AA$ = INKEY$
LOOP UNTIL AA$ <> ""
CLS
NAME$ = INI1$ + ". " + INI2$ + ". " + INI3$ + ". "
IF INI1$ = "" AND INI2$ = "" AND INI3$ = "" THEN NAME$ = "Ambush !!"
IF INI1$ = "G" AND INI2$ = "R" AND INI3$ = "M" THEN NAME$ = "Grim Reaper"
IF INI1$ = "L" AND INI2$ = "A" AND INI3$ = "O" THEN NAME$ = "Kung Lao"
IF INI1$ = "B" AND INI2$ = "O" AND INI3$ = "O" THEN NAME$ = "Casper"
IF INI1$ = "L" AND INI2$ = "A" AND INI3$ = "K" THEN NAME$ = "Brain"
IF INI1$ = "S" AND INI2$ = "K" AND INI3$ = "E" THEN NAME$ = "Smoke"
IF INI1$ = "S" AND INI2$ = "U" AND INI3$ = "B" THEN NAME$ = "Sub-zero"
IF INI1$ = "R" AND INI2$ = "E" AND INI3$ = "P" THEN NAME$ = "Reptile"
IF INI1$ = "S" AND INI2$ = "C" AND INI3$ = "O" THEN NAME$ = "Scorpian"
IF INI1$ = "D" AND INI2$ = "O" AND INI3$ = "G" THEN NAME$ = "Bailey"
IF INI1$ = "L" AND INI2$ = " " AND INI3$ = "M" THEN NAME$ = "Blood"
IF INI1$ = "J" AND INI2$ = "A" AND INI3$ = "H" THEN NAME$ = "Jimi Hendrix"
IF INI1$ = "E" AND INI2$ = "G" AND INI3$ = "G" THEN NAME$ = "The Eggman"
IF INI1$ = "S" AND INI2$ = "A" AND INI3$ = "N" THEN NAME$ = "Santa Clause"
IF INI1$ = "J" AND INI2$ = "T" AND INI3$ = "H" THEN NAME$ = "Jabba"
IF INI1$ = "M" AND INI2$ = " " AND INI3$ = "K" THEN NAME$ = "Great ones"
IF INI1$ = "M" AND INI2$ = "R" AND INI3$ = "C" THEN NAME$ = "Mr. Carlile"
IF INI1$ = "S" AND INI2$ = "L" AND INI3$ = "M" THEN NAME$ = "Slim Jim"
OPEN "FAME" FOR INPUT AS 1
INPUT #1, SCORE1%, SCORE2%, SCORE3%, SCORE4%, SCORE5%
INPUT #1, SCORE6%, SCORE7%, SCORE8%, SCORE9%, SCORE10%
INPUT #1, NAME1$, NAME2$, NAME3$, NAME4$, NAME5$
INPUT #1, NAME6$, NAME7$, NAME8$, NAME9$, NAME10$
INPUT #1, HITS1%, HITS2%, HITS3%, HITS4%, HITS5%
INPUT #1, HITS6%, HITS7%, HITS8%, HITS9%, HITS10%
CLOSE #1
IF SCORE >= SCORE1% THEN D% = 10
IF SCORE >= SCORE2% AND SCORE < SCORE1% THEN D% = 9: GOTO NXTF
IF SCORE >= SCORE3% AND SCORE < SCORE2% THEN D% = 8: GOTO NXTF
IF SCORE >= SCORE4% AND SCORE < SCORE3% THEN D% = 7: GOTO NXTF
IF SCORE >= SCORE5% AND SCORE < SCORE4% THEN D% = 6: GOTO NXTF
IF SCORE >= SCORE6% AND SCORE < SCORE5% THEN D% = 5: GOTO NXTF
IF SCORE >= SCORE7% AND SCORE < SCORE6% THEN D% = 4: GOTO NXTF
IF SCORE >= SCORE8% AND SCORE < SCORE7% THEN D% = 3: GOTO NXTF
IF SCORE >= SCORE9% AND SCORE < SCORE8% THEN D% = 2: GOTO NXTF
IF SCORE >= SCORE10% AND SCORE < SCORE9% THEN D% = 1: GOTO NXTF
IF SCORE <= SCORE10% THEN GOTO HALLOFFAME
IF SCORE = SCORE2% THEN D% = 9
IF SCORE = SCORE3% THEN D% = 8
IF SCORE = SCORE4% THEN D% = 7
IF SCORE = SCORE5% THEN D% = 6
IF SCORE = SCORE6% THEN D% = 5
IF SCORE = SCORE7% THEN D% = 4
IF SCORE = SCORE8% THEN D% = 3
IF SCORE = SCORE9% THEN D% = 2
NXTF:
IF D% = 1 THEN GOTO NXTH
SCORE10% = SCORE9%
NAME10$ = NAME9$
HITS10% = HITS9%
IF D% = 2 THEN GOTO NXTH
SCORE9% = SCORE8%
NAME9$ = NAME8$
HITS9% = HITS8%
IF D% = 3 THEN GOTO NXTH
SCORE8% = SCORE7%
NAME8$ = NAME7$
HITS8% = HITS7%
IF D% = 4 THEN GOTO NXTH
SCORE7% = SCORE6%
NAME7$ = NAME6$
HITS7% = HITS6%
IF D% = 5 THEN GOTO NXTH
SCORE6% = SCORE5%
NAME6$ = NAME5$
HITS6% = HITS5%
IF D% = 6 THEN GOTO NXTH
SCORE5% = SCORE4%
NAME5$ = NAME4$
HITS5% = HITS4%
IF D% = 7 THEN GOTO NXTH
SCORE4% = SCORE3%
NAME4$ = NAME3$
HITS4% = HITS3%
IF D% = 8 THEN GOTO NXTH
SCORE3% = SCORE2%
NAME3$ = NAME2$
HITS3% = HITS2%
IF D% = 9 THEN GOTO NXTH
SCORE2% = SCORE1%
NAME2$ = NAME1$
HITS2% = HITS1%
NXTH:
IF SCORE >= SCORE1% THEN SCORE1% = SCORE: NAME1$ = NAME$: HITS1% = HITS: GOTO HALLOFFAME
IF SCORE >= SCORE2% AND SCORE < SCORE1% THEN
SCORE2% = SCORE: NAME2$ = NAME$: HITS2% = HITS: CLR = 2: GOTO HALLOFFAME
END IF
IF SCORE >= SCORE3% AND SCORE < SCORE2% THEN
SCORE3% = SCORE: NAME3$ = NAME$: HITS3% = HITS: CLR = 3: GOTO HALLOFFAME
END IF
IF SCORE >= SCORE4% AND SCORE < SCORE3% THEN
SCORE4% = SCORE: NAME4$ = NAME$: HITS4% = HITS: CLR = 4: GOTO HALLOFFAME
END IF
IF SCORE >= SCORE5% AND SCORE < SCORE4% THEN
SCORE5% = SCORE: NAME5$ = NAME$: HITS5% = HITS: CLR = 5: GOTO HALLOFFAME
END IF
IF SCORE >= SCORE6% AND SCORE < SCORE5% THEN
SCORE6% = SCORE: NAME6$ = NAME$: HITS6% = HITS: CLR = 6: GOTO HALLOFFAME
END IF
IF SCORE >= SCORE7% AND SCORE < SCORE6% THEN
SCORE7% = SCORE: NAME7$ = NAME$: HITS7% = HITS: CLR = 7: GOTO HALLOFFAME
END IF
IF SCORE >= SCORE8% AND SCORE < SCORE7% THEN
SCORE8% = SCORE: NAME8$ = NAME$: HITS8% = HITS: CLR = 8: GOTO HALLOFFAME
END IF
IF SCORE >= SCORE9% AND SCORE < SCORE8% THEN
SCORE9% = SCORE: NAME9$ = NAME$: HITS9% = HITS: CLR = 9: GOTO HALLOFFAME
END IF
IF SCORE >= SCORE10% AND SCORE < SCORE9% THEN
SCORE10% = SCORE: NAME10$ = NAME$: HITS10% = HITS: CLR = 10: GOTO HALLOFFAME
END IF
HALLOFFAME:
CLS
SCREEN 13
C2 = 147
C1 = 33
DO
AA$ = INKEY$
COLOR 112
LOCATE 3, 10: PRINT "Most murderous men"
COLOR 43
LOCATE 5, 12: PRINT "Hall of Fame"
COLOR 15
LOCATE 8, 4: PRINT "No.   Initials"; SPACE$(8); "Score    Kills"
COLOR C1
LOCATE 10, 4: PRINT "#1.   "; NAME1$: LOCATE 10, 25
PRINT SCORE1%; "pts.": LOCATE 10, 35: PRINT HITS1%
COLOR C2
IF CLR = 2 THEN COLOR C1
LOCATE 11, 4: PRINT "#2.   "; NAME2$: LOCATE 11, 25
PRINT SCORE2%; "pts.": LOCATE 11, 35: PRINT HITS2%
COLOR C2
IF CLR = 3 THEN COLOR C1
LOCATE 12, 4: PRINT "#3.   "; NAME3$: LOCATE 12, 25
PRINT SCORE3%; "pts.": LOCATE 12, 35: PRINT HITS3%
COLOR C2
IF CLR = 4 THEN COLOR C1
LOCATE 13, 4: PRINT "#4.   "; NAME4$: LOCATE 13, 25
PRINT SCORE4%; "pts.": LOCATE 13, 35: PRINT HITS4%
COLOR C2
IF CLR = 5 THEN COLOR C1
LOCATE 14, 4: PRINT "#5.   "; NAME5$
LOCATE 14, 25: PRINT SCORE5%; "pts.": LOCATE 14, 35: PRINT HITS5%
COLOR C2
IF CLR = 6 THEN COLOR C1
LOCATE 15, 4: PRINT "#6.   "; NAME6$: LOCATE 15, 25
PRINT SCORE6%; "pts.": LOCATE 15, 35: PRINT HITS6%
COLOR C2
IF CLR = 7 THEN COLOR C1
LOCATE 16, 4: PRINT "#7.   "; NAME7$: LOCATE 16, 25
PRINT SCORE7%; "pts.": LOCATE 16, 35: PRINT HITS7%
COLOR C2
IF CLR = 8 THEN COLOR C1
LOCATE 17, 4: PRINT "#8.   "; NAME8$: LOCATE 17, 25
PRINT SCORE8%; "pts.": LOCATE 17, 35: PRINT HITS8%
COLOR C2
IF CLR = 9 THEN COLOR C1
LOCATE 18, 4: PRINT "#9.   "; NAME9$: LOCATE 18, 25
PRINT SCORE9%; "pts.": LOCATE 18, 35: PRINT HITS9%
COLOR C2
IF CLR = 10 THEN COLOR C1
LOCATE 19, 4: PRINT "#10.  "; NAME10$: LOCATE 19, 25
PRINT SCORE10%; "pts.": LOCATE 19, 35: PRINT HITS10%
COLOR C2
C1 = C1 + 1
C2 = C2 + 1
IF C1 >= 60 THEN C1 = 33
IF C2 >= 160 THEN C2 = 110
LOOP UNTIL AA$ <> ""
OPEN "FAME" FOR OUTPUT AS #1
WRITE #1, SCORE1%, SCORE2%, SCORE3%, SCORE4%, SCORE5%
WRITE #1, SCORE6%, SCORE7%, SCORE8%, SCORE9%, SCORE10%
WRITE #1, NAME1$, NAME2$, NAME3$, NAME4$, NAME5$
WRITE #1, NAME6$, NAME7$, NAME8$, NAME9$, NAME10$
WRITE #1, HITS1%, HITS2%, HITS3%, HITS4%, HITS5%
WRITE #1, HITS6%, HITS7%, HITS8%, HITS9%, HITS10%
CLOSE #1
COLOR 15
LOCATE 22, 15: PRINT "Play again ?"
DO
AA$ = INKEY$
AA$ = UCASE$(AA$)
LOOP UNTIL AA$ = "Y" OR AA$ = "N"
IF AA$ = "N" THEN COLOR 0: CLS : END
CLS
COLOR 15
LOCATE 10, 12: PRINT "Please wait while"
LOCATE 12, 12: PRINT "Ambush is loading"
CHAIN "AMBUSH.bas"
END SUB

SUB Gunswitch
SHARED GUN, BOSS, FLAME, BEANS, BAKEDBEANS, CHARACTER, NOGUNS, FLAMETROWER
SHARED CHARGE, FIRE, BEAN, AMMO, SNOT
IF GUN = 1 THEN
CHARGE = 0
IF BOSS = 50 THEN Charge1
LINE (257, 3)-(316, 21), 15, B
LINE (258, 4)-(315, 20), 4, BF
LINE (282, 5)-(289, 5), 114
LINE (282, 5)-(282, 12), 114
LINE (282, 12)-(278, 14), 114
LINE (278, 14)-(278, 17), 114
LINE (278, 17)-(292, 17), 114
LINE (292, 17)-(292, 14), 114
LINE (292, 14)-(289, 12), 114
LINE (289, 12)-(289, 5), 114
LINE (285, 9)-(285, 13), 114
LINE (284, 18)-(281, 20), 120, BF
LINE (280, 18)-(281, 20), 192
PAINT (282, 13), 65, 114
GUN = 2
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
GOTO ENDSUB2
END IF

IF GUN = NOGUNS THEN
IF CHARACTER = 5 THEN
LINE (257, 3)-(316, 21), 15, B: LINE (258, 4)-(315, 20), 4, BF
LINE (293 + 13, 61 - 42)-(245 + 13, 59 - 42), 113
LINE (245 + 13, 59 - 42)-(245 + 13, 57 - 42), 113
LINE (245 + 13, 57 - 42)-(296 + 13, 55 - 42), 113
LINE (296 + 13, 55 - 42)-(291 + 13, 50 - 42), 113
LINE (291 + 13, 50 - 42)-(291 + 13, 46 - 42), 113
LINE (291 + 13, 46 - 42)-(296 + 13, 47 - 42), 113
LINE (296 + 13, 47 - 42)-(301 + 13, 53 - 42), 113
LINE (301 + 13, 53 - 42)-(301 + 13, 58 - 42), 113
LINE (301 + 13, 58 - 42)-(299 + 13, 60 - 42), 113
LINE (299 + 13, 60 - 42)-(294 + 13, 60 - 42), 113
PAINT (296 + 13, 52 - 42), 114, 113
END IF
CHARGE = 0
IF BOSS = 50 THEN Charge1
IF CHARACTER <> 5 THEN
LINE (257, 3)-(316, 20), 15, B
LINE (258, 4)-(315, 20), 4, BF
LINE (262, 8)-(312, 8), 20
LINE (285, 9)-(312, 12), 24, BF
LINE (312, 6)-(312, 12), 20
LINE (312, 12)-(285, 12), 20
LINE (285, 12)-(285, 8), 20
LINE (285, 10)-(312, 10), 20
LINE (285, 15)-(276, 13), 20
LINE (285, 12)-(273, 13), 20
LINE (273, 13)-(262, 16), 20
LINE (262, 16)-(262, 8), 20
LINE (285, 15)-(289, 13), 20
PAINT (264, 9), 22, 20
END IF
GUN = 1
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
GOTO ENDSUB2
END IF
ENDSUB2:
END SUB

