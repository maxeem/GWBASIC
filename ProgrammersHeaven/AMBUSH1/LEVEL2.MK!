PAINT (131, 86), 1, 30: PAINT (131, 74), 4, 30
CIRCLE (135, 51), 1, 30, , , 1: CIRCLE (139, 51), 1, 30, , , 1
CIRCLE (137, 57), 2, 30, , , -.6: PAINT (137, 53), 65, 30
PAINT (70, 109), 136, 30: PAINT (313, 105), 136, 30: PAINT (313, 110), 136, 30
PAINT (6, 105), 136, 30: PAINT (6, 110), 136, 30: PAINT (33, 60), 24, 30
PAINT (33, 52), 26, 30: PAINT (262, 60), 26, 30: PAINT (268, 59), 24, 30
PAINT (138, 25), 8, 30: PAINT (270, 30), 9, 30: PAINT (30, 30), 9, 30
LINE (134, 45)-(139, 47), 6: LINE (139, 45)-(134, 47), 6
PSET (137, 54), 30
CIRCLE (X, Y - 90), 5, 6
GET (X - 5, Y - 95)-(X + 5, Y - 85), CIRC%
CIRCLE (X, Y - 90), 5, 0
Gunswitch
t:
TOILET = INT(RND * 28) + 1
PIZZA = INT(RND * 28) + 1
IF TOILET = 10 OR TOILET = 20 OR TOILET = 30 OR TOILET = 40 THEN
IF TOILET = 50 THEN GOTO t
END IF
IF PIZZA = 10 OR PIZZA = 20 OR PIZZA = 30 OR PIZZA = 40 OR PIZZA = 50 THEN
GOTO t
END IF
IF PIZZA = TOILET THEN GOTO t
LOCATE 1, 2: PRINT "Score :"; SCORE
LOCATE 1, 20: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
PUT (X - 5, Y - 5), CIRC%, XOR
X1 = 150: X2 = 160: Y1 = 9: Y2 = 18
CC = 16
FOR I = 1 TO CHARGE
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
X1 = X1 + 10: X2 = X2 + 10
CC = CC + 1.7
NEXT
X1 = 241: X2 = 249: Y1 = 9: Y2 = 18
FOR I = 1 TO 10 - CHARGE
LINE (X1, Y1)-(X2, Y2), 0, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), 0, BF
X1 = X1 - 10: X2 = X2 - 10
NEXT
INKY: Q$ = INKEY$
IF ADD2 > 0 THEN
PSET (SNOTTX, SNOTTY + ADD2), 120
PSET (SNOTTX + 1, SNOTTY + ADD1), 120
ADD1 = ADD1 + .05
ADD2 = ADD2 + .1
IF ADD2 >= 40 THEN ADD2 = 0: ADD1 = 0
END IF
IF OUCHYOU >= 10 THEN Gameover: CHAIN "AMBUSH.bas"
IF GUN = 1 AND CHARGE < 10 AND Q$ = "c" THEN ChargeC: GOTO INKY
LOCATE 1, 2: PRINT "Score :"; SCORE
LOCATE 1, 20: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES
IF HITS = 60 AND LEVEL = 2 THEN
LINE (0, 22)-(319, 171), 0, BF
LOCATE 10, 13: PRINT "GET PSYCHED !!!"
CHAIN "BOSS2.mk!"
END IF
IF HITS = TOILET THEN GOTO ITEMS
IF HITS = PIZZA THEN GOTO ITEMS
IF AMMOA = 0 AND HITS = NEWAMMO THEN GOTO AMMO
IF Q$ = "" AND AMMOA > SPD THEN GOSUB AMMOERASE
IF Q$ = "" AND AMMOA > 0 THEN AMMOA = AMMOA + 1: ENEMY = -1: ITEM = -1
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 0 AND ENEMYTYPE = 2 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + 1
GOSUB GETHIT: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 0 AND ENEMYTYPE = 1 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + .5
GOSUB GETHIT: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 500 AND ENEMYTYPE = 2 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + .75
GOSUB GETHIT: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > SPD AND ARMORA = 500 AND ENEMYTYPE = 1 THEN
GOSUB ENEMYERASE: OUCHYOU = OUCHYOU + .375
GOSUB GETHIT: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF Q$ = "" AND ENEMY > 0 THEN ENEMY = ENEMY + 1: AMMOA = -1: ITEM = -1
IF Q$ = "" AND ITEM > SPD THEN GOSUB ITEMERASE
IF Q$ = "" AND ITEM > 0 THEN ITEM = ITEM + 1: ENEMY = -1: AMMOA = -1
IF Q$ = "" AND ENEMY = 0 THEN GOTO ENEMY

N1:
    Q$ = UCASE$(Q$)
    DEF SEG = 0
    POKE 1047, Numlock
    DEF SEG
IF LEN(Q$) = 2 THEN GOTO NXT
Q$ = LCASE$(Q$)
IF Q$ = CHR$(27) THEN
GET (0, 21)-(319, 172), BIG%
LOCATE 10, 14: COLOR 15: PRINT "Are you sure ?"
DO
END$ = INKEY$
LOOP UNTIL END$ <> ""
PUT (0, 21), BIG%, PSET
IF UCASE$(LEFT$(END$, 1)) = "Y" THEN CLS : END
COLOR 14
GOTO INKY
END IF
IF Q$ = " " THEN GOSUB FIRE
IF Q$ = "p" THEN GOSUB PAUSE
IF Q$ = "s" THEN Gunswitch
IF Q$ = "b" AND BOMB > 0 THEN GOSUB BOOM
GOTO INKY
NXT:
IF LEN(Q$) = 2 THEN FK% = ASC(RIGHT$(Q$, 1))
IF FK% = 59 THEN GOTO ITEMSCREEN: GOTO INKY
IF FK% <> 71 AND FK% <> 72 AND FK% <> 73 AND FK% <> 75 AND FK% <> 77 THEN
IF FK% <> 79 AND FK% <> 80 AND FK% <> 81 THEN GOTO INKY
END IF
IF FK% = 71 AND X = 30 OR FK% = 71 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO INKY
END IF
IF FK% = 72 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO INKY
END IF
IF FK% = 73 AND X = 280 OR FK% = 73 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO INKY
END IF
IF FK% = 75 AND X = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR:  PUT (X - 5, Y - 5), CIRC%, XOR: GOTO INKY
END IF
IF FK% = 77 AND X = 280 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO INKY
END IF
IF FK% = 79 AND X = 30 OR FK% = 79 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO INKY
END IF
IF FK% = 80 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO INKY
END IF
IF FK% = 81 AND X = 280 OR FK% = 81 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO INKY
END IF
IF FK% = 71 THEN
PUT (X - 5, Y - 5), CIRC%, XOR:  X = X - 10
Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 72 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 73 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 75 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 77 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 79 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: X = X - 10
Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 80 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 81 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
GOTO INKY

PAUSE:
GET (0, 21)-(319, 172), BIG%
COLOR 15
LOCATE 10, 17: PRINT "PAUSE"
DO
AA$ = INKEY$
LOOP UNTIL UCASE$(AA$) = "P" OR UCASE$(AA$) = "S"
IF UCASE$(AA$) = "S" THEN PUT (0, 21), BIG%, PSET: Gunswitch: GOTO PAUSE
COLOR 14
PUT (0, 21), BIG%, PSET
RETURN

FIRE:
IF GUN = 1 AND AMMO = 0 THEN
PLAY "MBO6T255L64FG"
CHARGE = -.2: ChargeC
RETURN
END IF
IF GUN = 2 AND SNOT = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
IF FLAMETHROWER = 500 AND FIRE = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
FOR I = 1 TO CHARGE
PLAY "MBT255L64O0Cp64O0Dp64O0Cp64"
NEXT
GET (X - 20, Y - 20)-(X + 20, Y + 20), EXTRA%
IF CHARGE >= 1 THEN
HGLK = CHARGE
FOR I = 1 TO CHARGE + 10
CIRCLE (X, Y), HGLK + 10, HGLK + 21
HGLK = HGLK - 1
NEXT
PUT (X - 20, Y - 20), EXTRA%, PSET
END IF
IF LEVEL = 2 AND Y > 40 AND Y < 70 AND X > 20 AND X < 50 AND GUN = 1 THEN
IF BREAK = 0 THEN
CIRCLE (X, Y), 1, 0
LINE (X - 3, Y - 2)-(X + 2, Y), 0
LINE (X + 3, Y - 2)-(X - 2, Y + 1), 0
LINE (X - 1, Y - 3)-(X + 1, Y + 3), 0
LINE (27, 63)-(42, 68), 0: LINE (45, 60)-(26, 64), 0
LINE (27, 63)-(30, 69), 0: LINE (26, 64)-(23, 69), 0
LINE (25, 63)-(17, 68), 0: LINE (14, 60)-(27, 63), 0
LINE (27, 63)-(20, 59), 0: LINE (20, 59)-(20, 50), 0
LINE (20, 50)-(15, 44), 0: LINE (22, 44)-(24, 55), 0
LINE (24, 55)-(28, 62), 0: LINE (32, 44)-(34, 54), 0
LINE (34, 54)-(29, 62), 0: LINE (29, 62)-(38, 57), 0
LINE (38, 57)-(45, 44), 0
PLAY "T255MFL24O6BCEP32FGBCP42DFGP30BCDEGP52CEFG"
BREAK = 500
GGGGG = 2
END IF
END IF
IF LEVEL = 2 AND Y > 40 AND Y < 70 AND X > 250 AND X < 290 AND GUN = 1 THEN
IF BREAK1 = 0 THEN
CIRCLE (X, Y), 1, 0
LINE (X - 3, Y - 2)-(X + 2, Y), 0
LINE (X + 3, Y - 2)-(X - 2, Y + 1), 0
LINE (X - 1, Y - 3)-(X + 1, Y + 3), 0
LINE (258, 42)-(268, 49), 0: LINE (268, 49)-(278, 52), 0
LINE (278, 52)-(283, 48), 0: LINE (278, 53)-(282, 59), 0
LINE (277, 53)-(278, 64), 0: LINE (273, 64)-(278, 52), 0
LINE (276, 53)-(265, 65), 0: LINE (256, 61)-(277, 52), 0
LINE (277, 52)-(254, 53), 0: LINE (277, 52)-(272, 42), 0
PLAY "T255MFL24O6BCEP32FGBCP42DFGP30BCDEGP52CEFG"
BREAK1 = 500
GGGGG = 2
END IF
END IF
IF LEVEL = 2 AND Y > 40 AND Y < 70 AND X > 250 AND X < 290 AND GUN = 1 THEN
CIRCLE (X, Y), 1, 0
LINE (X - 3, Y - 2)-(X + 2, Y), 0
LINE (X + 3, Y - 2)-(X - 2, Y + 1), 0
LINE (X - 1, Y - 3)-(X + 1, Y + 3), 0
PLAY "T255MFL26O6BCEP14DBFG"
END IF
IF LEVEL = 2 AND Y > 40 AND Y < 70 AND X > 20 AND X < 50 AND GUN = 1 THEN
CIRCLE (X, Y), 1, 0
LINE (X - 3, Y - 2)-(X + 2, Y), 0
LINE (X + 3, Y - 2)-(X - 2, Y + 1), 0
LINE (X - 1, Y - 3)-(X + 1, Y + 3), 0
PLAY "T255MFL26O6BCEP14DBFG"
END IF
GET (X - 5, Y - 5)-(X + 5, Y + 5), HIT%
CIRCLE (X, Y), 3, 18
CIRCLE (X, Y), 2, 18
CIRCLE (X, Y), 1, 18
CIRCLE (X, Y), 3, 19
CIRCLE (X, Y), 2, 30
CIRCLE (X, Y), 1, 30
CIRCLE (X, Y), 3, 20
CIRCLE (X, Y), 2, 29
CIRCLE (X, Y), 1, 29
CIRCLE (X, Y), 3, 28
IF CHARACTER = 5 AND GUN = 1 THEN
PLAY "MBT255L32O5CEDFEGFAGBA>C<B>DCED": GGGGG = 2
END IF
IF BEANS = 0 THEN GGGGG = 2
IF GGGGG = 2 THEN GOTO W
IF GUN = 1 THEN PLAY "MBO0T255L64CDCDCDCDCDCD"
IF GUN = 2 THEN PLAY "T255O1L64CDEFG"
IF FLAMETHROWER = 500 THEN PLAY "T255MBL64O1CO0AO0CO0AO1AO0C"
W:
GGGGG = 0
PUT (X - 5, Y - 5), HIT%, PSET
IF X <> EROW AND Y < 280 AND Y > 20 AND GUN = 2 OR Y <> ECOL AND Y < 280 THEN
IF Y > 20 AND GUN = 2 THEN
CIRCLE (X, Y), 2, 120
PAINT (X, Y), 120
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 120
IF A = 2 THEN PSET (X - RX, Y + RY), 120
NEXT
END IF
END IF
IF X <> EROW AND Y < 280 AND Y > 20 AND CHARACTER = 5 THEN
IF GUN = 1 OR Y <> ECOL AND Y < 280 AND Y > 20 AND CHARACTER = 5 THEN
IF GUN = 1 THEN
GET (X - 20, Y - 10)-(X + 20, Y + 10), EXTRA%
Q = 4
M = 27
FOR I = 1 TO 5
CIRCLE (X - 4, Y - 4), Q, M, 6.283184, 4.712388, 1
