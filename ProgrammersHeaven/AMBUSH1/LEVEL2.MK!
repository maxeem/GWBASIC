LINE (EROW, ECOL + 3)-(EROW, ECOL + 4), 12
END IF
PUT (X - 5, Y - 5), CIRC%, XOR
ENEMY = 1
GOTO N1

ENEMYERASE:
IF ENEMY > 100 AND X <> EROW AND Y <> ECOL THEN MISSES = MISSES + 1
IF ENEMY > SPD AND ENEMYTYPE = 3 THEN
CIRCLE (EROW - 19, ECOL - 1), 7, 14, , , 1
CIRCLE (EROW + 21, ECOL - 1), 7, 14, , , 1
PAINT (EROW - 19, ECOL - 1), 14, 14
PAINT (EROW + 21, ECOL - 1), 14, 14
FOR I = 1 TO SPD - SPD / 1.5
NEXT
PLAY "T255MBO0L64CP64DP64CP64DP64CP64DP64CP64D"
END IF
PUT (X - 5, Y - 5), CIRC%, XOR
PUT (EROW - 27, ECOL - 9), ENEMY%, PSET
IF ENEMY > SPD AND X = EROW AND Y = ECOL THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF ENEMY < SPD AND X = EROW AND Y = ECOL THEN
PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF ENEMY < SPD AND X = EROW AND Y = ECOL AND BOMBING = 1 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
END IF
EROW = 0
ECOL = 0
ENEMY = 0
AMMOA = 0
RETURN

AMMO:
B:
