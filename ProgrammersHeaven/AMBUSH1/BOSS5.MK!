LINE (127, 53)-(130, 57), 28
PAINT (141, 74), 65, 28
PAINT (109, 76), 65, 28
PAINT (165, 92), 65, 28
PAINT (143, 61), 65, 28
PAINT (143, 93), 98, 28
PAINT (97, 60), 65, 28
PAINT (123, 152), 22, 28
PAINT (154, 152), 22, 28
PAINT (154, 160), 18, 28
PAINT (116, 160), 18, 28
PAINT (136, 47), 19, 28
PAINT (98, 54), 4, 28
PAINT (95, 54), 44, 28
PAINT (139, 62), 0, 28
GET (0, 21)-(319, 175), BIG%
X1 = 154: X2 = 169: Y1 = 26: Y2 = 44
CC = 195
FOR I = 1 TO 10
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
X1 = X1 + 15: X2 = X2 + 15
CC = CC + 1.7
NEXT
PUT (X - 5, Y - 5), CIRC%, XOR:
PLAY "T155MBL8O1F#FF#FED#EFF#FF#FED#EF"
BOSSINKY: Q$ = INKEY$
IF GUN = 1 AND CHARGE < 10 AND Q$ = "c" THEN Charge1: GOTO BOSSINKY
IF BOSS = 0 AND OUCH >= 10 THEN GOSUB KILLBOSS
IF BOSS = 300 AND OUCH >= 10 THEN GOSUB KILLBOSS1
IF BOSS = 0 AND OUCH >= 1 THEN GOSUB HITBOSS
IF OUCHYOU >= 10 THEN Gameover
IF GUNSHOOT >= SPD + SPD / 6 THEN GOSUB GUNSHOOT
IF GUNSHOOT < SPD + SPD / 6 THEN GUNSHOOT = GUNSHOOT + 1
LOCATE 1, 2: PRINT "Score :"; SCORE
LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
LOCATE 23, 2: PRINT "Dust  :"; : COLOR 14: PRINT AMMO
END IF
IF GUN = 2 THEN LOCATE 23, 2: PRINT "Snot  :"; SNOT
IF FLAMETHROWER = 500 THEN LOCATE 23, 2: PRINT "Fire  :"; FIRE
IF BAKEDBEANS = 500 THEN LOCATE 23, 2: PRINT "Beans :"; BEAN
IF BOWANDARROW = 500 THEN LOCATE 23, 2: PRINT "Arrows:"; ARROW
IF BAZOOKA = 500 THEN LOCATE 23, 2: PRINT "Missle:"; MISSLE
LOCATE 23, 15: PRINT "Kills :"; HITS
LOCATE 23, 27: PRINT "Misses :"; MISSES

N2:
    Q$ = UCASE$(Q$)
    DEF SEG = 0
    POKE 1047, Numlock
    DEF SEG
IF LEN(Q$) = 2 THEN GOTO NXT1
Q$ = LCASE$(Q$)
IF Q$ = " " THEN GOSUB FIRE
IF Q$ = "p" THEN GOSUB PAUSE
IF Q$ = "s" THEN Gunswitch
IF Q$ = "b" AND BOMB > 0 THEN GOSUB BOOM
IF Q$ = CHR$(27) THEN
GET (0, 21)-(319, 172), BIG%
LOCATE 10, 14: COLOR 15: PRINT "Are you sure ?"
DO
END$ = INKEY$
LOOP UNTIL END$ <> ""
PUT (0, 21), BIG%, PSET
IF UCASE$(LEFT$(END$, 1)) = "Y" THEN CLS : END
COLOR 14
GOTO BOSSINKY
END IF
GOTO BOSSINKY
NXT1:
IF LEN(Q$) = 2 THEN FK% = ASC(RIGHT$(Q$, 1))
IF FK% <> 71 AND FK% <> 72 AND FK% <> 73 AND FK% <> 75 THEN
IF FK% <> 77 AND FK% <> 79 AND FK% <> 80 AND FK% <> 81 THEN GOTO BOSSINKY
END IF
IF FK% = 71 AND X = 30 OR FK% = 71 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 72 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 73 AND X = 280 OR FK% = 73 AND Y = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 75 AND X = 30 THEN
PUT (X - 5, Y - 5), CIRC%, XOR:  PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 77 AND X = 280 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 79 AND X = 30 OR FK% = 79 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 80 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 81 AND X = 280 OR FK% = 81 AND Y = 160 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: PUT (X - 5, Y - 5), CIRC%, XOR: GOTO BOSSINKY
END IF
IF FK% = 71 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X - 10: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 72 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 73 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: Y = Y - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 75 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: X = X - 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 77 THEN
PUT (X - 5, Y - 5), CIRC%, XOR: X = X + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 79 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X - 10: Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 80 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF FK% = 81 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X = X + 10: Y = Y + 10: PUT (X - 5, Y - 5), CIRC%, XOR
END IF
GOTO BOSSINKY
PAUSE:
GET (0, 21)-(319, 172), BIG%
COLOR 15
LOCATE 10, 17: PRINT "PAUSE"
DO
AA$ = INKEY$
LOOP UNTIL UCASE$(AA$) = "P" OR UCASE$(AA$) = "S"
IF UCASE$(AA$) = "S" THEN PUT (0, 21), BIG%, PSET: Gunswitch: GOTO PAUSE
COLOR 14
PUT (0, 21), BIG%, PSET
RETURN

FIRE:
IF GUN = 1 AND AMMO = 0 THEN
PLAY "MBO6T255L64FG"
CHARGE = -.2
Charge1
RETURN
END IF
IF GUN = 2 AND SNOT = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
IF BAKEDBEANS = 500 AND BEAN = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
IF FLAMETHROWER = 500 AND FIRE = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
IF BOWANDARROW = 500 AND ARROW = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
IF BAZOOKA = 500 AND MISSLE = 0 THEN
PLAY "MBO6T255L64FG"
RETURN
END IF
IF BAZOOKA = 500 THEN CHARGE = 10
FOR I = 1 TO CHARGE
PLAY "MBT255L64O0Cp64O0Dp64O0Cp64"
NEXT
IF CHARGE >= 1 THEN
GET (X - 20, Y - 20)-(X + 20, Y + 20), EXTRA%
HGLK = CHARGE
FOR I = 1 TO CHARGE + 10
CIRCLE (X, Y), HGLK + 10, HGLK + 21
HGLK = HGLK - 1
NEXT
PUT (X - 20, Y - 20), EXTRA%, PSET
END IF
GET (X - 5, Y - 5)-(X + 5, Y + 5), HIT%
CIRCLE (X, Y), 3, 18
CIRCLE (X, Y), 2, 18
CIRCLE (X, Y), 1, 18
CIRCLE (X, Y), 3, 19
CIRCLE (X, Y), 2, 30
CIRCLE (X, Y), 1, 30
CIRCLE (X, Y), 3, 20
CIRCLE (X, Y), 2, 29
CIRCLE (X, Y), 1, 29
CIRCLE (X, Y), 3, 28
IF CHARACTER = 5 AND GUN = 1 THEN
PLAY "MBT255L32O5CEDFEGFAGBA>C<B>DCED": GGGGG = 2
END IF
IF GGGGG = 2 THEN GOTO W
IF GUN = 1 THEN PLAY "MBO0T255L64CDCDCDCDCDCD"
IF GUN = 2 THEN PLAY "T255O1L64CDEFG"
IF BAKEDBEANS = 500 THEN
PLAY "t255mbl64o0dp55cp55dp55cp55dp55cp55dp55cp55dp55cp55dp55c"
END IF
IF FLAMETHROWER = 500 THEN PLAY "T255MBL64O1CO0AO0CO0AO1AO0C"
IF BOWANDARROW = 500 THEN PLAY "T255L32O1CDEFEDCO0CDCDC"
IF BAZOOKA = 500 THEN PLAY "T255O0L44CDEFEDCDEFEDCDEFEDC"
W:
GGGGG = 0
PUT (X - 5, Y - 5), HIT%, PSET
IF GUN = 2 THEN
CIRCLE (X, Y), 2, 120
PAINT (X, Y), 120
FOR I = 1 TO 25
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 120
IF A = 2 THEN PSET (X - RX, Y + RY), 120
NEXT
END IF
IF CHARACTER = 5 AND GUN = 1 THEN
GET (X - 20, Y - 10)-(X + 20, Y + 10), EXTRA%
Q = 4
M = 27
FOR I = 1 TO 5
CIRCLE (X - 4, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X - 4, Y + 4), Q, M, 1.570796, 6.283184, 1
CIRCLE (X + 4, Y + 4), Q, M, 3.141592, 1.570796, 1
CIRCLE (X + 4, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X - 14, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X - 14, Y + 4), Q, M, 1.570796, 6.283184, 1
CIRCLE (X + 14, Y + 4), Q, M, 3.141592, 1.570796, 1
CIRCLE (X + 14, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X + 14, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X + 14, Y + 4), Q, M, 1.570796, 6.283184, 1
CIRCLE (X - 14, Y + 4), Q, M, 3.141592, 1.570796, 1
CIRCLE (X - 14, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X, Y), Q, M
CIRCLE (X - 9, Y), Q, M
CIRCLE (X + 9, Y), Q, M
Q = Q - 1
M = M + 1
NEXT
PUT (X - 20, Y - 10), EXTRA%, PSET
END IF
IF FLAMETHROWER = 500 THEN
PUT (X - 5, Y - 5), CIRC%
Q = 2.2: QQQ = .3: QQQQ = 33
FOR I = 1 TO 22
CIRCLE (X, Y + 1), QQQ, QQQQ, , , Q
QQQ = QQQ + .3: QQQQ = QQQQ + .5: Q = Q - .05
NEXT I
Q = 0: QQQ = .1: QQQQ = 36
FOR I = 1 TO 22
CIRCLE (X, Y), QQQ, QQQQ, , , Q
QQQ = QQQ + .39: QQQQ = QQQQ + .4: Q = Q + .1
NEXT I
PUT (X - 5, Y - 5), CIRC%
END IF
IF BAKEDBEANS = 500 THEN
PUT (X - 5, Y - 5), CIRC%
Q = 4
M = 25
FOR I = 1 TO 5
CIRCLE (X - 4, Y - 4), Q, M, 6.283184, 4.712388, 1
CIRCLE (X - 4, Y + 3), Q, M, 1.570796, 6.283184, 1
CIRCLE (X + 4, Y + 3), Q, M, 3.141592, 1.570796, 1
CIRCLE (X + 4, Y - 4), Q, M, 4.712388, 3.141592, 1
CIRCLE (X, Y), Q, M, , , 1
Q = Q - 1
M = M + 1.6
NEXT
NXT15:
PUT (X - 5, Y - 5), CIRC%
END IF
IF X = 110 AND Y = 80 AND BOSS = 0 THEN
IF G1 <> 500 AND GUN = 1 THEN G1 = 500: OUCH = OUCH + 1.7
IF G2 <> 500 AND GUN = 2 THEN G2 = 500: OUCH = OUCH + 1.7
IF G3 <> 500 AND FLAMETHROWER = 500 THEN G3 = 500: OUCH = OUCH + 1.7
IF G4 <> 500 AND BAKEDBEANS = 500 THEN G4 = 500: OUCH = OUCH + 1.7
IF G5 <> 500 AND BOWANDARROW = 500 THEN G5 = 500: OUCH = OUCH + 1.7
IF G6 <> 500 AND BAZOOKA = 500 THEN G6 = 500: OUCH = OUCH + 1.7
PLAY "MBT255L64O5AAEAA>CCACCEECEEeeceeceecee"
GOSUB HITBOSS
END IF
IF BAZOOKA = 500 AND X = 130 AND Y = 70 AND ACT = 0 THEN
OUCH = OUCH + .5
CIRCLE (X, Y), X3X, 4
X3X = X3X + 1
PAINT (X, Y), 4
FOR I = 1 TO 5
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 4
IF A = 2 THEN PSET (X - RX, Y + RY), 4
NEXT
IF OUCH = 3 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
X3X = 0
LINE (131, 76 - 13)-(92, 111 - 13), 0, BF
LINE (137, 123 - 13)-(185, 123 - 13), 29
LINE (137, 123 - 13)-(135, 84 - 13), 29
CIRCLE (134, 80 - 13), 4, 29, , , 1.2
PAINT (132, 80 - 13), 4, 29
LINE (137, 82 - 13)-(141, 98 - 13), 29
LINE (140, 98 - 13)-(140, 122 - 13), 29
PAINT (139, 119 - 13), 4, 29
CIRCLE (134, 80 - 13), 1, 29
CIRCLE (134, 80 - 13), 0, 31
PUT (X - 5, Y - 5), CIRC%, XOR
ACT = 1
END IF
END IF
IF BOSS = 300 AND ACT = 1 AND GUN = 1 AND X = 160 AND Y = 40 THEN
PUT (X - 5, Y - 5), CIRC%, XOR
IF OUCH >= 5 THEN
X3X = 0
LINE (145, 55 - 13)-(174, 36 - 13), 0, BF
LINE (150, 56 - 13)-(155, 53 - 13), 29
LINE (155, 53 - 13)-(162, 54 - 13), 29
LINE (162, 54 - 13)-(169, 50 - 13), 29
LINE (169, 50 - 13)-(170, 55 - 13), 29
LINE (169, 50 - 13)-(166, 48 - 13), 29
LINE (166, 48 - 13)-(159, 51 - 13), 29
LINE (159, 51 - 13)-(155, 50 - 13), 29
LINE (155, 50 - 13)-(149, 53 - 13), 29
LINE (149, 53 - 13)-(150, 56 - 13), 29
LINE (156, 54 - 13)-(165, 57 - 13), 29, B
PAINT (159, 52 - 13), 4, 29
CIRCLE (160, 38), 11, 29, 3.141, , 1
PAINT (160, 45), 4, 29
PAINT (160, 45), 65, 29
ACT = 2
PUT (X - 5, Y - 5), CIRC%, XOR
GOTO g
END IF
CIRCLE (X, Y), X3X, 4
X3X = X3X + .2
PAINT (X, Y), 4
FOR I = 1 TO 5
RX = INT(RND * 8) + 1
RY = INT(RND * 8) + 1
A = INT(RND * 2) + 1
IF RX <= 4 THEN RX = -RX
IF RY <= 4 THEN RY = -RX
IF RX >= 5 THEN RX = RX - 4
IF RY >= 5 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 4
IF A = 2 THEN PSET (X - RX, Y + RY), 4
NEXT
OUCH = OUCH + .1
PUT (X - 5, Y - 5), CIRC%, XOR
END IF
IF BOSS = 300 AND ACT = 2 AND X = 150 AND Y = 130 AND BOWANDARROW = 500 THEN
PUT (X - 5, Y - 5), CIRC%
CIRCLE (X, Y), X3X, 4
X3X = X3X + .25
PAINT (X, Y), 4
FOR I = 1 TO 5
RX = INT(RND * 8 + X3X) + 1
RY = INT(RND * 8 + X3X) + 1
A = INT(RND * 2) + 1
IF RX <= 4 + X3X / 2 THEN RX = -RX
IF RY <= 4 + X3X / 2 THEN RY = -RX
IF RX >= 5 + X3X / 2 THEN RX = RX - 4
IF RY >= 5 + X3X / 2 THEN RY = RX - 4
IF A = 1 THEN PSET (X + RX, Y - RY), 4
IF A = 2 THEN PSET (X - RX, Y + RY), 4
NEXT
OUCH = OUCH + .17
PUT (X - 5, Y - 5), CIRC%
BOWSHOT = 1
END IF
IF BOWSHOT = 0 AND BOWANDARROW = 500 THEN
CIRCLE (X, Y), 2, 29
LINE (X - 3, Y - 3)-(X + 3, Y + 3), 22
LINE (X + 3, Y - 3)-(X - 3, Y + 3), 22
LINE (X - 2, Y - 3)-(X + 4, Y + 3), 19
LINE (X + 4, Y - 3)-(X - 2, Y + 3), 19
LINE (X - 4, Y - 3)-(X + 2, Y + 3), 25
LINE (X + 2, Y - 3)-(X - 4, Y + 3), 25
END IF
BOWSHOT = 0
g:
IF GUN = 1 THEN AMMO = AMMO - 1
IF GUN = 2 THEN SNOT = SNOT - 1
IF FLAMETHROWER = 500 THEN FIRE = FIRE - 1
IF BAKEDBEANS = 500 THEN BEAN = BEAN - 1
IF BOWANDARROW = 500 THEN ARROW = ARROW - 1
IF BAZOOKA = 500 THEN MISSLE = MISSLE - 1
IF GUN = 1 AND CHARACTER = 8 THEN AMMO = AMMO + 1
IF GUN = 2 AND CHARACTER = 8 THEN SNOT = SNOT + 1
IF FLAMETHROWER = 500 AND CHARACTER = 8 THEN FIRE = FIRE + 1
IF BAKEDBEANS = 500 AND CHARACTER = 8 THEN BEAN = BEAN + 1
IF BOWANDARROW = 500 AND CHARACTER = 8 THEN ARROW = ARROW + 1
IF BAZOOKA = 500 AND CHARACTER = 8 THEN MISSLE = MISSLE + 1
IF GUN = 1 THEN CHARGE = -.2: Charge1
RETURN

BOOM:
GET (0, 21)-(319, 172), BIG%
FOR I = 1 TO 15
PLAY "MBT255L64O0Cp64O0Dp64O0Cp64"
NEXT
FOR NEWATTR = 0 TO 63
PALETTE 0, NEWATTR
FOR Q = 1 TO 25
NEXT
NEXT
FOR NEWATTR = 63 TO 0 STEP -1
PALETTE 0, NEWATTR
FOR Q = 1 TO 25
NEXT
NEXT
BOMBING = 1
BOMB = BOMB - 1
IF CHARACTER = 8 THEN BOMB = BOMB + 1
BOMBING = 0
RETURN

GETHIT:
GET (0, 21)-(319, 172), BIG%
PLAY "T255MBL64O0CDCDCDCDCDCDCDCDCDCDCDC"
PUT (0, 25), BIG%, PSET
PUT (0, 21), BIG%, PSET
LINE (0, 21)-(319, 24), 0, BF
LINE (0, 173)-(319, 200), 0, BF
LOCATE 2, 2: PRINT "Bombs :"; BOMB
IF GUN = 1 AND CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
IF GUN = 1 AND CHARACTER = 5 THEN
