DECLARE SUB Charge1 ()
DECLARE SUB Gameover ()
DECLARE SUB Center (ROW, TEXT$)
DECLARE SUB Gunswitch ()
COMMON NOGUNS, LEVEL, GUN, BOSS, X, Y, AMMO, SNOT, CHARGE, FLAME, BEANS
COMMON FLAMETHROWER, BAKEDBEANS, CHARACTER, CIRC%, BIG%, HIT%, EXTRA%
COMMON KEYB, FIRE, BEAN, HITS, MISSES, SCORE, BOMB, OUCHYOU, OUCH
COMMON INI, SPD, INI1$, INI2$, INI3$, TOILET, ENEMIES, ARMORA, BOW, ARROW
COMMON names, MISSLE, BAZOOKA, BZKA
RANDOMIZE TIMER
DIM ROW AS LONG
SCREEN 0
CLS
WIDTH 80
names = 500
COLOR 14, 8
Center 2, "Level 5: Your Mission"
COLOR 15, 8
IF names = 0 THEN
Center 8, " You are to defeat Mrs. Box and Mr. Wuss. This is the last level."
END IF
IF names = 500 THEN
Center 8, " You are to defeat Mrs. Cubic and Mr. Krivan. This is the last lev"
LOCATE 8, 74: PRINT "el."
END IF
Center 10, "Good Luck ! [ENTER]"
DO
AA$ = INKEY$
LOOP UNTIL AA$ = CHR$(13)
SCREEN 13
DIM CIRC%(1000)
DIM HIT%(1000)
DIM EXTRA%(1000)
DIM BIG%(25000)
'SPD = 100
'X = 150: Y = 80
NOGUNS = 6
GUN = 1
FLAME = 500
BEANS = 500
BOW = 500
BZKA = 500
AMMO = 50
SNOT = 50
FIRE = 20
BEAN = 20
ARROW = 70
MISSLE = 10
X = 150: Y = 80
CIRCLE (X, Y), 5, 15
GET (X - 5, Y - 5)-(X + 5, Y + 5), CIRC%
CIRCLE (X, Y), 5, 0
BOSS:
LOCATE 2, 1: PRINT STRING$(13, " ")
LOCATE 1, 1: PRINT STRING$(29, " ")
LINE (0, 21)-(319, 172), 0, BF
X1 = 104: X2 = 119: Y1 = 3: Y2 = 21
CC = 16
FOR I = 1 TO 10
LINE (X1, Y1)-(X2, Y2), 15, B
LINE (X1 + 1, Y1 + 1)-(X2 - 1, Y2 - 1), CC, BF
X1 = X1 + 15: X2 = X2 + 15
CC = CC + 1.7
NEXT
LINE (0, 22)-(319, 172), 0, BF
GOSUB HEALTH
GOSUB TALKBOSS
LINE (0, 22)-(319, 171), 0, BF
LINE (257, 3)-(316, 20), 15, B
LINE (258, 4)-(315, 20), 4, BF
LINE (262, 8)-(312, 8), 20
LINE (285, 9)-(312, 12), 24, BF
LINE (312, 6)-(312, 12), 20
LINE (312, 12)-(285, 12), 20
LINE (285, 12)-(285, 8), 20
LINE (285, 10)-(312, 10), 20
LINE (285, 15)-(276, 13), 20
LINE (285, 12)-(273, 13), 20
LINE (273, 13)-(262, 16), 20
LINE (262, 16)-(262, 8), 20
LINE (285, 15)-(289, 13), 20
PAINT (264, 9), 22, 20
GUN = 1
BAKEDBEANS = 0
FLAMETHROWER = 0
BOWANDARROW = 0
BAZOOKA = 0
IF CHARACTER <> 5 THEN LOCATE 23, 2: PRINT "Ammo  :"; AMMO
GOSUB HEALTH
PUT (X - 5, Y - 5), CIRC%, XOR
LINE (0, 22)-(319, 172), 0, BF
LINE (114, 162)-(127, 158), 28, B
LINE (127, 158)-(121, 151), 28, B
LINE (150, 151)-(156, 158), 28, B
LINE (150, 158)-(163, 162), 28, B
LINE (170, 151)-(108, 151), 28
LINE (120, 109)-(108, 151), 28
LINE (170, 151)-(158, 108), 28
LINE (158, 108)-(158, 83), 28
LINE (158, 83)-(166, 105), 28
LINE (166, 105)-(167, 119), 28
LINE (167, 119)-(171, 123), 28
LINE (171, 123)-(176, 120), 28
LINE (176, 120)-(174, 118), 28
LINE (174, 118)-(173, 100), 28
LINE (173, 100)-(167, 80), 28
LINE (167, 80)-(151, 71), 28
LINE (151, 71)-(144, 69), 28
LINE (143, 73)-(143, 66), 28
LINE (120, 111)-(120, 83), 28
LINE (120, 83)-(87, 83), 28
LINE (87, 83)-(87, 60), 28
LINE (96, 60)-(97, 73), 28
LINE (97, 73)-(118, 73), 28
LINE (118, 73)-(132, 69), 28
LINE (111, 73)-(111, 83), 28
LINE (161, 90)-(169, 87), 28
LINE (96, 62)-(98, 58), 28, B
LINE (96, 58)-(94, 55), 28
LINE (94, 55)-(87, 55), 28
LINE (87, 55)-(84, 58), 28
LINE (84, 58)-(87, 60), 28
LINE (97, 55)-(80, 53), 28, B
LINE (97, 53)-(99, 55), 28, B
LINE (80, 55)-(79, 54), 28
CIRCLE (138, 58), 9, 28, , , 1.2
LINE (140, 59)-(138, 59), 28
LINE (140, 59)-(138, 57), 28
